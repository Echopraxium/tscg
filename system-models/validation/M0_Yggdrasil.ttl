@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ns1: <m0:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<m0:yggdrasil:DynamicAgent_Nidhogg> a owl:NamedIndividual ;
    rdfs:label "Nídhögg - Degradation Agent" ;
    skos:altLabel "Malice Striker" ;
    ns1:behavior "Constantly chews roots, feeds on corpses in Niflheim" ;
    ns1:counterpart "m0:yggdrasil:DynamicAgent_Nornes" ;
    ns1:equilibriumRole "Without Nídhögg, system would stagnate (no renewal pressure). His gnawing is constructive stress." ;
    ns1:function "Gnaws at Yggdrasil roots (especially Hvergelmir root), representing entropy and dissolution" ;
    ns1:principle "Destruction pole of Cyclic Tension - necessary counterpart to Nornes' restoration" ;
    ns1:role "Perpetual degradation force" ;
    ns1:symbolicMeaning "Entropy, dissolution, return to primordial undifferentiation" ;
    ns1:systemRole [ ] .

<m0:yggdrasil:DynamicAgent_Nornes> a owl:NamedIndividual ;
    rdfs:label "Nornes - Restoration Agents" ;
    skos:altLabel "Fates (Urd, Verdandi, Skuld)" ;
    ns1:counterpart "m0:yggdrasil:DynamicAgent_Nidhogg" ;
    ns1:dutyIntegration "Weave Wyrd (web of fate) from threads of all events across all poles" ;
    ns1:dutyRestoration "Draw water from Urðarbrunnr to water tree roots daily, maintaining vitality" ;
    ns1:equilibriumRole "Maintain tree viability against Nídhögg's gnawing. Balance restoration rate = degradation rate at system equilibrium." ;
    ns1:function "Water Yggdrasil roots daily (restoration) + weave Wyrd (destiny) from events" ;
    ns1:members [ ] ;
    ns1:principle "Construction pole of Cyclic Tension + holistic integration" ;
    ns1:role "Perpetual restoration force + fate weaving" ;
    ns1:systemRole [ ] .

<m0:yggdrasil:DynamicAgent_Ratatosk> a owl:NamedIndividual ;
    rdfs:label "Ratatosk - Information Circulation Agent" ;
    skos:altLabel "Drill-Tooth Squirrel" ;
    ns1:behavior "Runs up and down tree trunk carrying provocative messages" ;
    ns1:endpoints "Eagle (at Tree Crown)",
        "Nídhögg (dragon at Niflheim roots)" ;
    ns1:function "Circulates messages (often rumor/dissonance) between lowest pole (Nídhögg at roots) and highest pole (Eagle at crown)" ;
    ns1:messageType "Rumor, insults, dissonance (creates productive tension)" ;
    ns1:principle "Prevents any pole from isolating - maintains communication network integrity" ;
    ns1:role "Information flux vector between system extremes" ;
    ns1:significance "Embodies Circulation principle - perpetual information flow maintaining system coherence despite pole differences" ;
    ns1:systemRole [ ] .

<m0:yggdrasil:Pole_Alfheim> a owl:NamedIndividual,
        <m0:yggdrasil:SevenPoleSystem> ;
    rdfs:label "Álfheim - Inspiration & Fluidity" ;
    skos:altLabel "Pole of Ethereal Knowledge" ;
    ns1:asfidContribution [ ] ;
    ns1:complementaryPole "m0:yggdrasil:Pole_Jotunheim" ;
    ns1:description "If Jötunheim is 'heavy' wisdom, Álfheim is 'light' knowledge. Pole of pure art, intuitive thought, connection to subtle currents. Light elves are emanations. Counterbalances materiality of Midgard and potential rigidity of Ásgard." ;
    ns1:elementalAssociation "Light, air, ethereal substance" ;
    ns1:energy "Soft light, creative impulse, grace" ;
    ns1:function "Inspiration, lightness, intuition, beauty, ethereal connection" ;
    ns1:keyEntities "Freyr (associated deity - rules Álfheim)",
        "Ljósálfar (light elves)" ;
    ns1:ontologicalCategory "Inspiration / Art / Subtle Perception" ;
    ns1:temporalAspect "Timeless (eternal present of inspiration)" ;
    ns1:wisdomType "'Light' wisdom - artistic, intuitive, ethereal" .

<m0:yggdrasil:Pole_Jotunheim> a owl:NamedIndividual,
        <m0:yggdrasil:SevenPoleSystem> ;
    rdfs:label "Jötunheim - Wisdom & Resonance" ;
    skos:altLabel "Pole of Wild Knowledge" ;
    ns1:asfidContribution [ ] ;
    ns1:complementaryPole "m0:yggdrasil:Pole_Alfheim" ;
    ns1:description "Often seen as chaotic enemy, but here pole of untamed knowledge. Giants (Jötunn) represent primordial nature forces and wisdom anterior to gods. Odin draws knowledge here. Pole of resonance with raw cosmos, necessary to prevent order (pole 4) from atrophying." ;
    ns1:elementalAssociation "Stone, mountains, wilderness, raw nature" ;
    ns1:energy "Telluric intelligence, cunning, demonic scale" ;
    ns1:function "Ancient knowledge, chaotic-but-fertile forces, primordial memory, cosmic resonance" ;
    ns1:keyEntities "Jötnar (giants) - primordial beings",
        "Mimir (wisest being, guardian of well)",
        "Mímisbrunnr (Well of Wisdom)" ;
    ns1:odinConnection "Odin sacrifices eye to Mimir for wisdom from this pole" ;
    ns1:ontologicalCategory "Wild Wisdom / Chaos / Ancient Memory" ;
    ns1:temporalAspect "Deep past (pre-cosmic knowledge)" ;
    ns1:wisdomType "'Heavy' wisdom - ancient, telluric, pre-rational" .

<m0:yggdrasil:Pole_Midgard> a owl:NamedIndividual,
        <m0:yggdrasil:SevenPoleSystem> ;
    rdfs:label "Midgard/Ásgard/Vanaheim - Manifestation & Order" ;
    skos:altLabel "Pole of Manifest Reality" ;
    ns1:asfidContribution [ ] ;
    ns1:bifröstConnection "Rainbow bridge connecting Midgard ↔ Ásgard (vertical integration structure)" ;
    ns1:description "Not single world but vertical axis. Midgard (manifest plane, horizontal) is field of experience. Ásgard (Aesir gods) is 'high' pole of order and conscious structure. Vanaheim (Vanir gods) is complementary pole bringing fertility, cycles, organic connection. Together form level of ordered, inhabited reality." ;
    ns1:elementalAssociation "Earth (Midgard), Air (Ásgard), Water (Vanaheim)" ;
    ns1:energy "Dynamic equilibrium, vertical sovereignty, organic cycles" ;
    ns1:function "Expression, law, exchange, organized consciousness, civilization" ;
    ns1:internalStructure [ ] ;
    ns1:keyEntities "Aesir: Odin, Thor, Frigg (Ásgard)",
        "Bifröst (rainbow bridge to Ásgard)",
        "Humans (Midgard)",
        "Vanir: Freyr, Freyja, Njörðr (Vanaheim)" ;
    ns1:ontologicalCategory "Manifestation / Order / Conscious Experience" ;
    ns1:temporalAspect "Present (lived experience)" .

<m0:yggdrasil:Pole_Muspellheim> a owl:NamedIndividual,
        <m0:yggdrasil:SevenPoleSystem> ;
    rdfs:label "Muspellheim - Impulse & Conflict" ;
    skos:altLabel "Pole of Primordial Fire" ;
    ns1:asfidContribution [ ] ;
    ns1:description "Opposite and complementary pole to Niflheim. Initial 'engine', expansive force pushing formation and action. Without it, system would be inertia. Fire giants are guardians. Represents spark, disruptive charge necessary for all evolution." ;
    ns1:dialecticalFunction "Generative opposition with Niflheim creates Ymir (primordial giant) → cosmos emerges from tension" ;
    ns1:elementalAssociation "Fire, heat, light, radiation" ;
    ns1:energy "Fire, radiation, irresistible impulse" ;
    ns1:function "Primary energy, expansion, catalysis, creative destruction" ;
    ns1:keyEntities "Ragnarök role (final conflagration)",
        "Surtr (fire giant leader)" ;
    ns1:ontologicalCategory "Impulse / Energy / Expansion" ;
    ns1:oppositionPole "m0:yggdrasil:Pole_Niflheim" ;
    ns1:temporalAspect "Initiation (moment of beginning)" .

<m0:yggdrasil:Pole_Niflheim> a owl:NamedIndividual,
        <m0:yggdrasil:SevenPoleSystem> ;
    rdfs:label "Niflheim/Helheim - Origins & Memory" ;
    skos:altLabel "Pole of Primordial Memory" ;
    ns1:asfidContribution [ ] ;
    ns1:complementaryPoles "m0:yggdrasil:Pole_Svartalfheim" ;
    ns1:description "Pole of 'the generic' - undifferentiated primordial matter. Contains the past, decomposed forms, but also potential for rebirth. Source Hvergelmir springs here, feeding all roots. Cosmic hard drive, root anchoring system in anteriority." ;
    ns1:elementalAssociation "Ice, mist, primordial waters" ;
    ns1:energy "Static cold, latent memory, potentiality" ;
    ns1:function "Conservation, primordial memory, dissolution, recycling" ;
    ns1:keyEntities "Helheim (realm of the dead)",
        "Hvergelmir (spring source)",
        "Nídhögg (root-gnawing dragon)" ;
    ns1:ontologicalCategory "Origins / Memory / Dissolution" ;
    ns1:oppositionPole "m0:yggdrasil:Pole_Muspellheim" ;
    ns1:temporalAspect "Past (what has been dissolved)" .

<m0:yggdrasil:Pole_Svartalfheim> a owl:NamedIndividual,
        <m0:yggdrasil:SevenPoleSystem> ;
    rdfs:label "Svartalfheim/Nidavellir - Formation & Modeling" ;
    skos:altLabel "Pole of Hidden Architecture" ;
    ns1:asfidContribution [ ] ;
    ns1:complementaryPoles "m0:yggdrasil:Pole_Midgard",
        "m0:yggdrasil:Pole_Niflheim" ;
    ns1:description "Underground workshop of the system. Dwarves (or dark elves) forge models and structures supporting visible worlds. Code source, master plan, pole transforming raw material (from Niflheim) into organized forms." ;
    ns1:elementalAssociation "Earth, metal, forge fire (controlled)" ;
    ns1:energy "Compression, patient craftsmanship, pressure" ;
    ns1:function "Structuration, algorithmic processing, modeling, pattern forging" ;
    ns1:keyEntities "Artifacts forged: Mjölnir, Gungnir, Draupnir",
        "Dwarves (Dvergar) - master craftsmen" ;
    ns1:ontologicalCategory "Formation / Algorithm / Hidden Structure" ;
    ns1:temporalAspect "Atemporality (eternal patterns)" .

<m0:yggdrasil:Pole_TreeCrown> a owl:NamedIndividual,
        <m0:yggdrasil:SevenPoleSystem> ;
    rdfs:label "Tree Crown / Urd's Well - Synthesis & Destiny" ;
    skos:altLabel "Pole of Ultimate Perspective" ;
    ns1:asfidContribution [ ] ;
    ns1:description "Point of view of Tree itself. Symbolized by eagle perched at crown and falcon Vedfolnir between its eyes (seeing all). Includes Urd's Well where Nornes weave destiny (Wyrd) while watering roots. Not place of power but ultimate perspective. Integrates information from all other poles to maintain global equilibrium (destiny)." ;
    ns1:elementalAssociation "Void/Aether (transcends elements)" ;
    ns1:energy "Impartiality, silence above fray, acceptance" ;
    ns1:fateMechanism "Nornes weave Wyrd (destiny) by integrating information from all poles, then water roots to maintain tree (feedback loop)" ;
    ns1:function "Global vision, fate, judgment, integration, destiny weaving" ;
    ns1:internalStructure [ ] ;
    ns1:keyEntities "Eagle (unnamed) - omniscient observer",
        "Nornes (Urd, Verdandi, Skuld) - fate weavers",
        "Urðarbrunnr (Well of Fate)",
        "Vedfolnir (hawk) - perfect perception" ;
    ns1:ontologicalCategory "Synthesis / Fate / Omniscience" ;
    ns1:temporalAspect "Eternal (beyond time - sees all temporal threads)" ;
    ns1:visionScope "Sees entire system from outside and above - meta-perspective" .

<m0:yggdrasil:SystemicTension> a owl:NamedIndividual ;
    rdfs:label "Yggdrasil Systemic Tension" ;
    rdfs:comment "Perpetual state of dynamic equilibrium through opposing forces. Tree is never 'finished' - constantly gnawed and restored. This stress is normal condition, not pathology." ;
    ns1:equilibriumCondition "Rate(restoration) = Rate(degradation) at stable state S*" ;
    ns1:formula "dS/dt = R(S) - D(S) where S* satisfies R(S*) = D(S*)" ;
    ns1:impermanence "System exists *because* of tension, not despite it. Removal of either pole → collapse." ;
    ns1:m1Instantiation "m1:core:CyclicTension" ;
    ns1:opposingForces [ ] ;
    ns1:philosophicalImplication "Being is becoming. Stasis = death. Life requires perpetual struggle for balance." .

<m0:yggdrasil:ThreeRoots> a owl:Class ;
    rdfs:label "Yggdrasil's Three Functional Roots" ;
    rdfs:comment "Not merely geographical but functional. Each root draws from specific source (memory, conflict, destiny) and feeds entire network. Triadic foundation enabling 7-pole superstructure." ;
    ns1:principleTriadic "Three roots = triadic stable foundation (cf. Fire Triangle N=3). Seven poles = expanded complexity built on this base." ;
    ns1:rootStructure [ ] .

<m0:yggdrasil:Yggdrasil_Poclet> a owl:Ontology ;
    dcterms:created "2026-01-22" ;
    dcterms:creator "Echopraxium with the collaboration of Claude AI" ;
    dcterms:description "TSCG modeling of Yggdrasil, the World Tree of Norse mythology, as a 7-pole multipolar network maintaining cosmic equilibrium through perpetual energy circulation. First TSCG poclet in mythology/cosmology domain, extending N-ary polarity to N=7." ;
    dcterms:modified "2026-01-22" ;
    dcterms:title "Yggdrasil - Norse Cosmological Tree (TSCG Poclet)" ;
    rdfs:seeAlso "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/poclets/yggdrasil/M0_Yggdrasil_README.md" ;
    owl:imports "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M1_CoreConcepts.jsonld",
        "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M2_MetaConcepts.jsonld" ;
    owl:versionInfo "1.0.0" ;
    ns1:asfidAnalysis [ ] ;
    ns1:changelog [ ] ;
    ns1:corePrinciple "Cosmic equilibrium maintained through perpetual circulation of energies (waters, wind, information) among seven interdependent poles. No pole is superior; each contributes essential function. System viability depends on dynamic tension between destruction (Nídhögg) and restoration (Nornes)." ;
    ns1:epistemicGap [ ] ;
    ns1:m1Contributions [ ] ;
    ns1:m2CandidatesProposed [ ] ;
    ns1:oriveAnalysis [ ] ;
    ns1:pocletMetadata [ ] ;
    ns1:systemArchitecture [ ] ;
    ns1:tscgClassification [ ] .

<m0:yggdrasil:SevenPoleSystem> a owl:Class ;
    rdfs:label "Yggdrasil Seven-Pole Cosmological System" ;
    rdfs:comment "Complete cosmological architecture organized around 7 interdependent poles (worlds) connected by World Tree Yggdrasil. Each pole represents distinct ontological category; system equilibrium arises from pole interactions mediated by dynamic agents (Ratatosk, Nornes, Nídhögg)." ;
    rdfs:subClassOf "m1:core:MultipolarNetwork" ;
    ns1:systemProperties [ ] .

