        "m2:Observer" ;
    m0:s5Functions "Arbitrate S3 vs S4 deadlocks",
        "Create and maintain organizational culture",
        "Define mission, vision, values",
        "Ensure ethical governance",
        "Establish constitutional ground rules",
        "Monitor systemic health and viability" ;
    m0:vsmMetaphor "The ultimate authority - defining who we are" ;
    m0:vsmRole "Policy & Identity" .

