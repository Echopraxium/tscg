        "m0:vsm:System4",
        "m0:vsm:System5" ;
    m0:instantiatesM2 "m2:Adaptation",
        "m2:Hierarchy",
        "m2:Homeostasis",
        "m2:Imbrication",
        "m2:Resilience",
        "m2:System" .

