ns1:ColorSynthesisFederated a owl:Ontology ;
    dcterms:created "2026-01-17" ;
    dcterms:creator "TSCG Project - Michel Favre" ;
    dcterms:description "Federating ontology for color synthesis models. References four specialized poclets: RGB (additive), HSL (additive), CMY (subtractive), CMYK (subtractive). Demonstrates multiple modeling approaches for same phenomenon (perceived color)." ;
    dcterms:modified "2026-01-17" ;
    dcterms:references "Adobe (2000). PostScript Language Reference (3rd ed.) - CMYK color model",
        "CIE (Commission Internationale de l'Ã‰clairage) standards",
        "Fairchild, M. D. (2013). Color Appearance Models (3rd ed.)",
        "Poynton, C. (2003). Digital Video and HD: Algorithms and Interfaces" ;
    dcterms:title "Color Synthesis - Federated Poclet Ontology" ;
    rdfs:seeAlso ns1:CMYK_Subtractive,
        ns1:CMY_Subtractive,
        ns1:FireTriangle,
        ns1:HSL_Additive,
        ns1:RGB_Additive ;
    owl:imports "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M1_CoreConcepts.jsonld",
        "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M2_MetaConcepts.jsonld" ;
    owl:versionInfo "1.0.0" ;
    m2:ontologyCategory "Poclet" ;
    ns1:comparisonDimensions [ ] ;
    ns1:domain "Optics / Color Science" ;
    ns1:federatedInstances ns1:CMYK_Subtractive,
        ns1:CMY_Subtractive,
        ns1:HSL_Additive,
        ns1:RGB_Additive ;
    ns1:futureExtensions [ ] ;
    ns1:implementationConsiderations [ ] ;
    ns1:metaconceptsDistinct [ ] ;
    ns1:metaconceptsShared [ ] ;
    ns1:numberOfVariants 4 ;
    ns1:pedagogicalValue [ ] ;
    ns1:phenomenon "Color Perception and Synthesis" ;
    ns1:pocletType "Federated" ;
    ns1:summary [ ] ;
    ns1:territoryVsMapAnalysis [ ] ;
    ns1:transdisciplinaryPrinciple [ ] ;
    ns1:transformationNetwork [ ] .

