      "m0:fusionPrinciple": {
        "metaconcept": "m2:Fusion",
        "aspect": "Fusion (subtractive mixing + black addition)",
        "formula": "S⊗F⊗D",
        "description": "Four ink layers fuse through cumulative absorption (CMY) + opaque black overlay (K)",
        "operation": "White_paper → C_ink → M_ink → Y_ink → K_ink → Reflected_spectrum → Perceived_color",
        "emergentProperty": "High-quality printed color with true blacks",
        "physicalMechanism": "Spectral filtering (CMY) + black masking (K)",
        "halftoning": "Inks applied as dots (halftone screening) - human eye fuses dots into continuous tone",
        "syntheticColors": [
          {
            "name": "Pure Black",
            "composition": "K(100%)",
            "CMYK": "(0%, 0%, 0%, 100%)",
            "result": "True deep black (opaque ink)",
            "note": "No CMY needed - K alone"
          },
          {
            "name": "Rich Black",
            "composition": "C(60%) ⊕ M(40%) ⊕ Y(40%) ⊕ K(100%)",
            "CMYK": "(60%, 40%, 40%, 100%)",
            "result": "Very deep, rich black (CMY adds depth)",
            "use": "Large black areas, backgrounds"
          },
          {
            "name": "Red",
            "composition": "M(100%) ⊕ Y(100%) ⊕ K(0%)",
            "CMYK": "(0%, 100%, 100%, 0%)",
            "result": "Bright red"
          },
          {
            "name": "Green",
            "composition": "C(100%) ⊕ Y(100%) ⊕ K(0%)",
            "CMYK": "(100%, 0%, 100%, 0%)",
            "result": "Bright green"
          },
          {
            "name": "Blue",
            "composition": "C(100%) ⊕ M(100%) ⊕ K(0%)",
            "CMYK": "(100%, 100%, 0%, 0%)",
            "result": "Bright blue"
          },
          {
            "name": "White",
            "composition": "C(0%) ⊕ M(0%) ⊕ Y(0%) ⊕ K(0%)",
            "CMYK": "(0%, 0%, 0%, 0%)",
            "result": "Paper white (no ink)"
          }
        ]
      },
      "m0:blackGenerationStrategies": {
        "description": "Methods to convert CMY to CMYK (how much K to use)",
        "strategies": [
          {
            "name": "GCR (Gray Component Replacement)",
            "principle": "Replace common CMY component with K",
            "algorithm": "K = min(C, M, Y); C' = C-K, M' = M-K, Y' = Y-K",
            "advantage": "Maximum ink savings, consistent grays",
            "use": "General commercial printing"
          },
          {
            "name": "UCR (Under Color Removal)",
            "principle": "Remove CMY only under dark areas (where K is present)",
            "algorithm": "K = min(C, M, Y) × factor; Reduce CMY proportionally in shadows",
            "advantage": "Better color saturation, less ink savings than GCR",
            "use": "High-quality color reproduction"
          },
          {
            "name": "Maximum K",
            "principle": "Use K wherever possible, minimize CMY",
            "advantage": "Maximum cost savings, fastest drying",
            "disadvantage": "Reduced color gamut",
            "use": "Newspapers, low-cost printing"
          },
          {
            "name": "Minimum K",
            "principle": "Use K only for deep shadows, maximize CMY",
            "advantage": "Widest color gamut",
            "disadvantage": "Highest ink cost, slower drying",
            "use": "Fine art reproduction"
          }
        ]
      },
      "m0:halftoning": {
        "metaconcept": "m2:Code",
        "principle": "Continuous tone simulated by discrete ink dots",
        "mechanism": "Variable dot size/spacing creates illusion of shades",
        "techniques": [
          {
            "name": "AM (Amplitude Modulated) Screening",
            "method": "Fixed dot spacing, variable dot size",
            "use": "Traditional offset printing",
            "lpi": "133-175 lines per inch (typical)"
          },
          {
            "name": "FM (Frequency Modulated) Screening",
            "method": "Fixed dot size, variable dot spacing (stochastic)",
            "advantage": "No moiré patterns, finer detail",
            "use": "High-quality digital printing"
          }
