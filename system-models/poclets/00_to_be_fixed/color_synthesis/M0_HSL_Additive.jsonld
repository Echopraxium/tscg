      "m0:fusionPrinciple": {
        "metaconcept": "m2:Fusion",
        "aspect": "Fusion (perceptual integration via RGB transformation)",
        "formula": "S⊗F⊗D",
        "description": "Three perceptual channels (H, S, L) fuse through transformation to RGB, then electromagnetic wave superposition produces perceived color",
        "operation": "HSL → Transform_to_RGB → R_wave ⊕ G_wave ⊕ B_wave → FUSION → Perceived_Color",
        "emergentProperty": "Perceived color (qualia)",
        "physicalMechanism": "Same as RGB (wave superposition), but specified in perceptual coordinates",
        "perceptualAdvantage": "More intuitive for humans than RGB primaries",
        "syntheticColors": [
          {
            "name": "Pure Red",
            "fusion": "H(0°) ⊕ S(100%) ⊕ L(50%)",
            "HSL": "(0°, 100%, 50%)",
            "RGB_equivalent": "(255, 0, 0)",
            "result": "Fully saturated red at mid-lightness"
          },
