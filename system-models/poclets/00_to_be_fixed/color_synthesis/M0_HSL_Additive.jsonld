      "m0:transformationToRGB": {
        "metaconcept": "m2:Transformation",
        "formula": "D⊗I⊗S",
        "reversible": true,
        "lossless": true,
        "algorithm": {
          "HSL_to_RGB": [
            "C = (1 - |2L - 1|) × S  // Chroma",
            "X = C × (1 - |(H/60°) mod 2 - 1|)",
            "m = L - C/2",
            "(R',G',B') depends on H sector (0-60°, 60-120°, etc.)",
            "(R,G,B) = ((R'+m)×255, (G'+m)×255, (B'+m)×255)"
          ],
          "RGB_to_HSL": [
            "Normalize: R'=R/255, G'=G/255, B'=B/255",
            "M = max(R',G',B'), m = min(R',G',B')",
            "L = (M + m)/2",
            "If M=m: H undefined, S=0 (achromatic)",
            "Else: S = (M-m)/(1-|2L-1|)",
            "H from which channel is max"
          ]
        }
      },
