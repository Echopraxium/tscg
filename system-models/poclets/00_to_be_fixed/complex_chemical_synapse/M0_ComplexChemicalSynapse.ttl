        <m1:core:Poclet> ;
    rdfs:label "Blood Pressure Homeostasis (5-Effector System)" ;
    rdfs:comment "Long-term regulation of arterial blood pressure through coordinated action of RAAS, Sympathetic NS, ADH, Natriuretic Peptides, and Cardiac Output. Set-point: ~120/80 mmHg in humans. Pathology: Chronic hypertension from dysregulation." ;
    ns2:asfid_analysis [ ns3:A [ ] ;
            ns3:D [ ] ;
            ns3:F [ ] ;
            ns3:I [ ] ;
            ns3:S [ ] ] ;
    ns2:bicephalous_perspective [ ] ;
    ns2:effector_count 5 ;
    ns2:effectors <m0:blood_pressure_control:ADH>,
        <m0:blood_pressure_control:CardiacOutput>,
        <m0:blood_pressure_control:NatriureticPeptides>,
        <m0:blood_pressure_control:RAAS>,
        <m0:blood_pressure_control:Sympathetic> ;
    ns2:epistemic_gap [ ] ;
    ns2:future_extensions "Add endothelial regulation (NO, endothelin)",
        "Include genetic polymorphisms (renin, ACE, angiotensinogen genes)",
        "Link to chronic kidney disease (bidirectional causality)",
        "Model circadian variation (BP dipping at night)",
        "Separate poclets: Cardiac Output (Starling curve), RAAS Cascade, Baroreceptor Reflex" ;
    ns2:gamification_potential [ ] ;
    ns2:m1_concepts_used [ ns4:Endocrine "Circulating aldosterone, ADH, ANP" ;
            ns4:Paracrine "Local angiotensin II in kidneys and vessels" ;
            ns5:BaroreflexArc "PROPOSED - Baroreceptors → Vasomotor center → Sympathetic output" ;
            ns5:CardiacOutput "PROPOSED - CO = HR × SV" ;
            ns5:PressureNatriuresis "PROPOSED - Intrinsic renal pressure-dependent sodium excretion" ;
