    {
      "@id": "m0:blood_pressure_control:RAAS_Effector",
      "@type": "tscg:EffectorSystem",
      "rdfs:label": "Renin-Angiotensin-Aldosterone System (RAAS)",
      "rdfs:comment": "Hormonal cascade controlling blood volume and vasoconstriction",
      "tscg:timescale": "minutes to hours",
      "tscg:mechanismType": "hormonal",
      "tscg:primaryAction": "Volume regulation and vasoconstriction",
      "tscg:keyComponents": [
        "Renin (from juxtaglomerular cells)",
        "Angiotensinogen → Angiotensin I → Angiotensin II",
        "Aldosterone (from adrenal cortex)",
        "AT1 receptors (vascular, renal)"
      ],
      "tscg:pathologicalRole": "Hyperactivity causes hypertension (renovascular hypertension model)",
      "tscg:therapeuticTargets": [
        "ACE inhibitors (block Ang I → Ang II)",
        "AT1 receptor blockers (ARBs)",
        "Aldosterone antagonists"
      ],
      "tscg:feedbackLoop": "Negative (Ang II inhibits renin secretion)",
      "tscg:clinicalSignificance": "Primary target for antihypertensive therapy"
    },
    {
      "@id": "m0:blood_pressure_control:Sympathetic_Effector",
      "@type": "tscg:EffectorSystem",
      "rdfs:label": "Sympathetic Nervous System",
      "rdfs:comment": "Rapid neural reflex modulating heart rate and vascular tone",
      "tscg:timescale": "seconds",
      "tscg:mechanismType": "neural",
      "tscg:primaryAction": "Heart rate control and vasoconstriction",
      "tscg:keyComponents": [
        "Baroreceptors (aortic arch, carotid sinus)",
        "Medullary cardiovascular centers",
        "Sympathetic efferents (norepinephrine release)",
        "Beta-1 adrenergic receptors (heart)",
        "Alpha-1 adrenergic receptors (blood vessels)"
      ],
      "tscg:pathologicalRole": "Chronic activation contributes to hypertension and heart failure",
      "tscg:therapeuticTargets": [
        "Beta-blockers (block β1 receptors)",
        "Alpha-blockers (block α1 receptors)",
        "Central sympatholytics"
      ],
      "tscg:feedbackLoop": "Negative (baroreceptor reflex: high pressure → reduced sympathetic output)",
      "tscg:adaptation": "Baroreceptor resetting in chronic hypertension (loss of reflex buffering)"
    },
    {
      "@id": "m0:blood_pressure_control:ADH_Effector",
      "@type": "tscg:EffectorSystem",
      "rdfs:label": "Anti-Diuretic Hormone (Vasopressin)",
      "rdfs:comment": "Water retention and vasoconstriction via hypothalamic-pituitary axis",
      "tscg:timescale": "minutes to hours",
      "tscg:mechanismType": "hormonal",
      "tscg:primaryAction": "Water reabsorption and vasoconstriction",
      "tscg:keyComponents": [
        "Hypothalamic osmoreceptors and baroreceptors",
        "Posterior pituitary (ADH release)",
        "V1 receptors (vascular smooth muscle - vasoconstriction)",
        "V2 receptors (renal collecting duct - aquaporin-2 insertion)"
      ],
      "tscg:pathologicalRole": "SIADH (inappropriate ADH secretion) causes water retention and hyponatremia",
      "tscg:therapeuticTargets": [
        "V2 antagonists (vaptans - for hypervolemia)",
        "Desmopressin (synthetic ADH for diabetes insipidus)"
      ],
      "tscg:feedbackLoop": "Negative (volume expansion inhibits ADH release)"
    },
    {
      "@id": "m0:blood_pressure_control:ANP_Effector",
      "@type": "tscg:EffectorSystem",
      "rdfs:label": "Atrial Natriuretic Peptide (ANP/BNP)",
      "rdfs:comment": "Counter-regulatory hormones promoting sodium and water excretion",
      "tscg:timescale": "minutes to hours",
      "tscg:mechanismType": "hormonal",
      "tscg:primaryAction": "Natriuresis, diuresis, and vasodilation",
      "tscg:keyComponents": [
        "Atrial stretch receptors",
        "ANP (from atrial myocytes)",
        "BNP (from ventricular myocytes)",
        "NPR-A receptors (kidney, blood vessels)",
        "cGMP second messenger pathway"
      ],
      "tscg:pathologicalRole": "ANP resistance in heart failure; used as biomarker (BNP levels)",
      "tscg:therapeuticTargets": [
        "Neprilysin inhibitors (prevent ANP/BNP degradation)",
        "Synthetic natriuretic peptides (nesiritide for acute heart failure)"
      ],
      "tscg:feedbackLoop": "Negative (ANP opposes RAAS and ADH; volume expansion triggers ANP release)",
      "tscg:counterRegulatory": "Balances RAAS/ADH effects"
    },
    {
      "@id": "m0:blood_pressure_control:CardiacOutput_Effector",
      "@type": "tscg:EffectorSystem",
      "rdfs:label": "Cardiac Output & Blood Volume",
      "rdfs:comment": "Hemodynamic downstream parameters integrating effects of effectors 1-4",
      "tscg:timescale": "seconds to days",
      "tscg:mechanismType": "hemodynamic",
      "tscg:primaryAction": "Blood pressure = Cardiac Output × Total Peripheral Resistance",
      "tscg:keyComponents": [
        "Heart (contractility, heart rate - Starling mechanism)",
        "Blood volume (regulated by kidney - pressure-natriuresis curve)",
        "Venous return (Frank-Starling law)",
        "Total peripheral resistance (vascular tone)"
      ],
      "tscg:pathologicalRole": "High cardiac output in early hypertension; increased resistance in established hypertension",
      "tscg:therapeuticTargets": [
        "Diuretics (reduce blood volume)",
        "Vasodilators (reduce peripheral resistance)",
        "Inotropes (modify contractility)"
      ],
      "tscg:hemodynamicEquation": "MAP = CO × TPR, where CO = Heart Rate × Stroke Volume",
      "tscg:guytonModel": "Long-term BP regulation via pressure-natriuresis relationship"
    },
