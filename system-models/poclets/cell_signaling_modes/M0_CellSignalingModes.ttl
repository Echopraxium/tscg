@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix m2: <https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M2_MetaConcepts.jsonld#> .
@prefix ns1: <m0:> .
@prefix ns2: <m0:check_following_attributes:> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<m0:cell_signaling_modes:CellSignalingModes> a owl:NamedIndividual,
        "m0:Poclet" ;
    rdfs:label "Cell Signaling Modes (Hormonal Communication)" ;
    dcterms:created "2026-01-20" ;
    dcterms:creator "Echopraxium with collaboration of Claude AI Pro" ;
    dcterms:references "Alberts, B., et al. (2014). Molecular Biology of the Cell (6th ed.) - Chapter 15: Cell Signaling",
        "Boron, W. F., & Boulpaep, E. L. (2016). Medical Physiology (3rd ed.)",
        "Guyton, A. C., & Hall, J. E. (2015). Textbook of Medical Physiology (13th ed.)",
        "Hadley, M. E., & Levine, J. E. (2006). Endocrinology (6th ed.)",
        "Lodish, H., et al. (2016). Molecular Cell Biology (8th ed.) - Chapter 16: Signaling Pathways",
        "Norris, D. O., & Carr, J. A. (2013). Vertebrate Endocrinology (5th ed.)" ;
    rdfs:comment "Fundamental biological system demonstrating BALANCE (Territory) between four modes of cellular communication: Autocrine, Paracrine, Neuroendocrine, and Endocrine. The system exhibits TRADE-OFFs (Map) between signal transmission speed and spatial range. Canonical pedagogical model in endocrinology and cell biology, analogous to Fire Triangle in combustion and Exposure Triangle in photography." ;
    rdfs:seeAlso ns1:ExposureTriangle,
        ns1:FireTriangle ;
    owl:imports "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M1_CoreConcepts.jsonld",
        "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M1_extensions/biology/M1_Biology.jsonld",
        "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M2_MetaConcepts.jsonld",
        "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M3_GenesisSpace.jsonld" ;
    owl:versionInfo "1.0.0" ;
    m2:ontologyCategory "Poclet" ;
    ns1:alternativeNames "Cell-Cell Communication Types",
        "Hormonal Signaling Pathways",
        "Modes of Intercellular Communication" ;
    ns1:analogyToFireAndExposureTriangles [ ] ;
    ns1:balancePrinciple [ ] ;
    ns2:END "" ;
    ns2:START "" ;
    ns1:components ns1:AutocrineSignaling,
        ns1:EndocrineSignaling,
        ns1:NeuroendocrineSignaling,
        ns1:ParacrineSignaling ;
    ns1:domain "Endocrinology / Cell Biology / Physiology" ;
    ns1:epistemicGap [ ] ;
    ns1:futureExtensions [ ] ;
    ns1:mapSpace [ ] ;
    ns1:metaconceptsMobilized [ ] ;
    ns1:oriveAnalysis [ ] ;
    ns1:pedagogicalRole "Universal teaching model for intercellular communication modes" ;
    ns1:physicalBasis "Molecular diffusion and blood circulation as transport mechanisms for hormonal signals" ;
    ns1:pocletType "Canonical (Pedagogical 4-Component System)" ;
    ns1:principle "Distance-Dependent Signal Transmission" ;
    ns1:territorySpace [ ] ;
    ns1:tradeoffPrinciple [ ] ;
    ns1:transdisciplinaryAnalogies [ ] ;
    ns1:validation [ ] .

ns1:AutocrineSignaling a m2:Component ;
    rdfs:label "Autocrine Signaling" ;
    ns1:asfidContribution [ ] ;
    ns1:diffusionMechanism "Local concentration gradient around secreting cell" ;
    ns1:examples "Growth factors in cancer cells (self-stimulation)",
        "Neurotransmitter autoreceptors (synaptic modulation)",
        "Prostaglandins in smooth muscle (local control)",
        "T-cell activation via IL-2 (immune response)" ;
    ns1:function "Local feedback regulation and cell growth control" ;
    ns1:metaconceptInstantiation "m2:Component" ;
    ns1:physicalCorrelate "Hormone binds to receptors on same cell that secreted it" ;
    ns1:physiologicalContexts [ ] ;
    ns1:role "Self-signaling - cell responds to its own secreted signals" ;
    ns1:spatialRange "Intracellular to immediate cell surface (~1-10 µm)" ;
    ns1:temporalRange "Milliseconds to seconds" ;
    ns1:tensorFormula "S⊗I⊗F (Structure × Information × Flow)" ;
    ns1:tradeoffProfile [ ] .

ns1:EndocrineSignaling a m2:Component ;
    rdfs:label "Endocrine Signaling (Classic)" ;
    ns1:asfidContribution [ ] ;
    ns1:examples "Cortisol from adrenal cortex (stress, metabolism)",
        "Estrogen/Testosterone from gonads (sexual characteristics)",
        "Growth hormone from pituitary (growth, metabolism)",
        "Insulin from pancreas (glucose homeostasis)",
        "Thyroxine (T4) from thyroid (metabolic rate)" ;
    ns1:feedbackControl [ ] ;
    ns1:function "Coordinate physiology across entire organism (metabolism, growth, reproduction)" ;
    ns1:metaconceptInstantiation "m2:Component" ;
    ns1:physicalCorrelate "Endocrine gland secretes hormone → blood → target organs with receptors" ;
    ns1:physiologicalContexts [ ] ;
    ns1:role "Systemic signaling - hormone travels through bloodstream to distant targets" ;
    ns1:spatialRange "Systemic (entire organism)" ;
    ns1:temporalRange "Minutes to hours (hormone half-life dependent)" ;
    ns1:tensorFormula "S⊗I⊗F⊗A (Structure × Information × Flow × Attractor)" ;
    ns1:tradeoffProfile [ ] ;
    ns1:transportMechanism "Blood circulation (convective transport, not diffusion)" .

ns1:NeuroendocrineSignaling a m2:Component ;
    rdfs:label "Neuroendocrine Signaling" ;
    ns1:asfidContribution [ ] ;
    ns1:examples "ADH/Vasopressin (water retention, blood pressure)",
        "Adrenaline/Noradrenaline from adrenal medulla (fight-or-flight)",
        "Growth hormone-releasing hormone (GHRH → GH)",
        "Oxytocin release (childbirth, lactation, social bonding)" ;
    ns1:function "Rapid neural trigger with sustained systemic endocrine effect" ;
    ns1:hybridNature "Combines neural speed with endocrine range" ;
    ns1:metaconceptInstantiation "m2:Component" ;
    ns1:physicalCorrelate "Hypothalamic neurons release hormones into posterior pituitary → circulation" ;
    ns1:physiologicalContexts [ ] ;
    ns1:role "Neural-to-systemic bridge - neurons secrete hormones into bloodstream" ;
    ns1:spatialRange "Systemic (entire body via bloodstream)" ;
    ns1:temporalRange "Seconds (neural) → Minutes to hours (endocrine)" ;
    ns1:tensorFormula "S⊗I⊗F⊗D (Structure × Information × Flow × Dynamics)" ;
    ns1:tradeoffProfile [ ] ;
    ns1:uniqueFeatures [ ] .

ns1:ParacrineSignaling a m2:Component ;
    rdfs:label "Paracrine Signaling" ;
    ns1:asfidContribution [ ] ;
    ns1:diffusionMechanism "Concentration gradient in interstitial fluid (Fick's laws)" ;
    ns1:examples "Growth factors in wound healing (fibroblast activation)",
        "Neurotrophic factors in brain (neuron survival)",
        "Nitric oxide (NO) in blood vessels (vasodilation)",
        "Prostaglandins in inflammation (local vasodilation)" ;
    ns1:function "Coordinate activity of cell populations in tissue microenvironment" ;
    ns1:gradientDynamics [ ] ;
    ns1:metaconceptInstantiation "m2:Component" ;
    ns1:physicalCorrelate "Hormone diffuses through extracellular fluid to neighboring cells" ;
    ns1:physiologicalContexts [ ] ;
    ns1:role "Local neighborhood signaling - affects nearby cells" ;
    ns1:spatialRange "Local tissue microenvironment (~10 µm - 1 mm)" ;
    ns1:temporalRange "Seconds to minutes" ;
    ns1:tensorFormula "S⊗I⊗F⊗∇ (Structure × Information × Flow × Gradient)" ;
    ns1:tradeoffProfile [ ] .

