      "m0:processorArchitecture": {
        "inputs": [
          {
            "@id": "m0:Kidneys/input/blood",
            "rdfs:label": "Blood (Renal Artery)",
            "m0:valueSpace": "fluid",
            "m0:flowRate": "~1200 mL/min (20-25% of cardiac output)",
            "m0:composition": [
              "water",
              "electrolytes (Na+, K+, Cl-, HCO3-)",
              "waste (urea, creatinine, uric acid)",
              "glucose",
              "amino acids",
              "proteins",
              "cells"
            ],
            "m0:pressure": "~100 mmHg (arterial)"
          },
          {
            "@id": "m0:Kidneys/input/water",
            "rdfs:label": "Water (from blood plasma)",
            "m0:valueSpace": "fluid",
            "m0:volume": "~180 L/day filtered",
            "m0:role": "Medium for filtration and solute transport"
          }
        ],
        "outputs": [
          {
            "@id": "m0:Kidneys/output/filtered_blood",
            "rdfs:label": "Filtered Blood (Renal Vein)",
            "m0:valueSpace": "fluid",
            "m0:flowRate": "~1200 mL/min",
            "m0:composition": [
              "water",
              "electrolytes (balanced)",
              "glucose (reabsorbed)",
              "amino acids (reabsorbed)",
              "proteins (retained)",
              "cells (retained)",
              "waste (reduced by ~99%)"
            ],
            "m0:pressure": "~8 mmHg (venous)"
          },
          {
            "@id": "m0:Kidneys/output/urine",
            "rdfs:label": "Urine (to bladder)",
            "m0:valueSpace": "fluid",
            "m0:volume": "~1.5 L/day (0.8% of filtered water)",
            "m0:composition": [
              "water (95%)",
              "urea (~9 g/L)",
              "creatinine (~0.8 g/L)",
              "uric acid (~0.4 g/L)",
              "electrolytes (excess Na+, K+, Cl-)",
              "H+ ions (pH regulation)"
            ],
            "m0:osmolality": "50-1200 mOsm/kg (variable concentration)"
          }
        ],
        "processingStages": [
          {
            "@id": "m0:Kidneys/stage/glomerular_filtration",
            "rdfs:label": "Glomerular Filtration",
            "m0:mechanism": "Passive filtration through glomerular capillaries",
            "m0:rate": "~125 mL/min (GFR - Glomerular Filtration Rate)",
            "m0:selectivity": "Size-based: passes molecules <70 kDa, retains proteins and cells",
            "m0:m2basis": "m2:Filter"
          },
          {
            "@id": "m0:Kidneys/stage/tubular_reabsorption",
            "rdfs:label": "Tubular Reabsorption",
            "m0:mechanism": "Active and passive transport from tubule → blood",
