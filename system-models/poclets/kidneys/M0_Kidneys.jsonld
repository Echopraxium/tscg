            "m0:m2basis": "m2:Resource"
          },
          {
            "@id": "m0:Kidneys/stage/tubular_secretion",
            "rdfs:label": "Tubular Secretion",
            "m0:mechanism": "Active transport from blood → tubule",
            "m0:secreted": "H+ ions, K+, organic acids, drugs, toxins",
            "m0:role": "Fine-tune composition, eliminate drugs/toxins",
            "m0:m2basis": "m2:Transformation"
          }
        ]
      },
