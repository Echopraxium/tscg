<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net10.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <RootNamespace>TSCG.Orchestrator</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <!-- Exclude all .cs files in _archives folder from compilation -->
    <Compile Remove="_archives\**\*.cs" />
    <None Include="_archives\**\*.cs" />
  </ItemGroup>

</Project>