    dcterms:created "2026-01-22" ;
    dcterms:creator "Echopraxium with the collaboration of Claude AI" ;
    rdfs:comment "System organized around N â‰¥ 3 distinct poles (nodes, centers, principles) with interdependent relationships forming a coherent whole. Unlike bipolar systems (duality) or hierarchies (tree), multipolar networks distribute significance across multiple equivalent or complementary poles without central dominance. Each pole contributes essential function; removing any pole destabilizes the entire system." ;
    ns2:M2_basis "m2:Attractor (A) - System equilibrium across poles",
        "m2:Information (I) - Distinctions between pole identities",
        "m2:Structure (S) - Topological organization of N poles" ;
    ns2:characteristicProperties [ ] ;
    ns2:discoveryContext [ ] ;
    ns2:distinctionFromRelated [ ] ;
    ns2:domainScope "Mythology, Game Design, Education, Political Science, Ethics, Complex Systems Theory" ;
    ns2:examplesCrossDomain "Complex Systems: Multi-agent systems, distributed networks",
        "Education: TPACK (N=7 knowledge domains)",
        "Ethics: Moral Foundations Theory (N=6 foundations)",
        "Game Design: Magic Color Wheel (N=5 colors), Rock-Paper-Scissors (N=3)",
        "Mythology: Yggdrasil (N=7 worlds), Greek Pantheon (N=12 Olympians)",
        "Political Science: Political Compass (N=4 quadrants), Tripartite Government (N=3 branches)" ;
    ns2:mathematicalProperties [ ] ;
    ns2:principle "Distributed equilibrium across N â‰¥ 3 interdependent poles where each pole is irreducible and contributes unique function to system stability" ;
    ns2:status "VALIDATED - Discovered through Yggdrasil analysis, confirmed in Magic Color Wheel and TPACK" ;
    ns2:tensorFormula "SâŠ—IâŠ—A" ;
    ns2:validatedPoclets 3 ;
    ns2:validatedPocletsDetails "Magic Color Wheel (N=5) - Game Design / Philosophy",
        "TPACK (N=7) - Education",
        "Yggdrasil (N=7) - Mythology" .

<m1:core:RelationalPattern> a owl:Class ;
    rdfs:label "Relational Pattern" ;
    rdfs:comment "Generic type of relationship between entities. Examples: Competition (species, companies, ideas), Cooperation (mutualism, trade, collaboration), Parasitism (biological, economic extraction)." ;
    rdfs:subClassOf "m1:core:CoreConcept" ;
    skos:note "To be populated with specific patterns as they are discovered through poclet analysis." .

ns1:CascadeAmplification a owl:Class ;
    rdfs:label "Cascade Amplification" ;
    rdfs:comment "Sequential multi-stage process where each stage has gain > 1, resulting in exponential amplification. Combines Cascade (staged sequence) with Amplification (gain). Examples: neutron multiplication, transistor amplifiers, MAPK signaling, photomultiplier tubes." ;
    skos:definition "Multi-stage process where signal, energy, or population is amplified at each stage (Gᵢ > 1), producing overall gain = ∏ᵢ Gᵢ >> 1. Subset of Cascade metaconcept with specific amplification requirement." ;
    skos:related "m1core:SelfSustainingReaction",
        "m2:Amplification",
        "m2:Cascade" ;
    ns1:controlChallenges "Exponential growth difficult to control (small perturbations → large effects)",
        "Latency issues (cascade takes time, feedback delayed)",
        "Negative feedback essential (prevent runaway)",
        "Requires precise regulation (nuclear: k=1.000, not 1.001)" ;
    ns1:designConsiderations "Feedback: Negative feedback essential for stable operation",
        "Gain per stage (Gᵢ): Higher gain → fewer stages needed but stability issues",
        "Saturation: Each stage has maximum output (nonlinearity)",
        "Stage count (n): More stages → higher total gain but longer latency" ;
    ns1:discoveryContext "Nuclear fission neutron multiplication (cascade with gain >1) revealed universal pattern of sequential amplification across domains." ;
    ns1:m2Basis "m2:Amplification",
        "m2:Cascade" ;
    ns1:mathematicalModel [ ] ;
    ns1:tensorFormula "Cascade ⊗ Amplification (gain > 1 per stage)" ;
    ns1:transdisciplinaryExamples [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ] ;
    ns1:validationCount 6 .

ns1:CriticalityRegime a owl:Class ;
    rdfs:label "Criticality Regime" ;
    rdfs:comment "System state relative to self-sustaining threshold. Three regimes: subcritical (process dies), critical (self-sustaining equilibrium), supercritical (exponential growth). Universal pattern appearing when process products enable continuation of process." ;
    skos:definition "Classification of system state based on relationship to critical threshold where process becomes self-sustaining (reproduction number = 1, multiplication factor k = 1, percolation probability p = p_c)." ;
    skos:related "m1core:SelfSustainingReaction",
        "m2:Bifurcation",
        "m2:Threshold" ;
    ns1:discoveryContext "Emerged from Nuclear Reactor Typology poclet analysis. Pattern recognized as universal across all self-sustaining processes with threshold behavior." ;
    ns1:m2Basis "m2:Bifurcation",
        "m2:Homeostasis",
        "m2:Threshold" ;
    ns1:mathematicalStructure [ ] ;
    ns1:regimes [ ] ;
    ns1:tensorFormula "Threshold ⊗ SelfSustainingReaction ⊗ Amplification" ;
    ns1:transdisciplinaryDomains "Chemistry (autocatalytic reactions)",
        "Ecology (population dynamics)",
        "Economics (growth/decay rates)",
        "Epidemiology (disease reproduction number R₀)",
        "Nuclear Engineering (fission criticality)",
        "Percolation Theory (bond/site probability)",
        "Phase Transitions (temperature vs T_critical)",
        "Social Networks (viral spread)" ;
    ns1:validationCount 8 .

ns1:DualCircuitArchitecture a owl:Class ;
    rdfs:label "Dual Circuit Architecture" ;
    rdfs:comment "System design with separated primary and secondary circuits connected via interface/barrier. Function: isolation (safety, contamination), efficiency (heat exchange), or voltage transformation. Examples: PWR reactor (radioactive primary, clean secondary), HVAC (refrigerant/air), electrical transformer (high/low voltage)." ;
    skos:definition "Architectural pattern with two distinct circuits separated by barrier, enabling transfer (energy, information, material) while maintaining isolation (safety, pressure, voltage)." ;
    skos:related "m2:Flow",
        "m2:Interface",
        "m2:Structure" ;
    ns1:architecturalComponents [ ] ;
    ns1:designAdvantages "Efficiency (optimize each circuit independently)",
        "Maintenance (secondary circuit accessible, primary sealed)",
        "Pressure decoupling (different operating pressures)",
        "Safety (contamination/radiation contained in primary)" ;
    ns1:discoveryContext "PWR nuclear reactor primary/secondary circuit separation revealed universal architectural pattern for safe energy transfer." ;
    ns1:keyProperties "Barrier interface (heat exchanger, transformer, membrane)",
        "Isolation guarantee (safety, contamination prevention)",
        "Physical separation (two distinct circuits)",
        "Transfer mechanism (thermal, electrical, chemical)" ;
    ns1:m2Basis "m2:Flow",
        "m2:Interface",
        "m2:Structure" ;
    ns1:tensorFormula "Structure ⊗ Interface ⊗ Isolation" ;
    ns1:transdisciplinaryExamples [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ] ;
    ns1:validationCount 6 .

ns1:ModeratorMechanism a owl:Class ;
    rdfs:label "Moderator Mechanism" ;
    rdfs:comment "Component or process that slows, controls, or stabilizes a rapid/intense process. Function: energy reduction, rate control, stabilization. Examples: neutron moderator (nuclear), enzyme inhibitor (biology), damper (mechanics), low-pass filter (electronics)." ;
    skos:definition "Regulatory element that reduces process intensity or rate without stopping it entirely. Acts as intermediate control between full activity and complete shutdown." ;
    skos:related "m1core:SelfSustainingReaction",
        "m2:Amplification",
        "m2:Regulation" ;
    ns1:designPrinciples "Continuous action (not binary on/off)",
        "Energy dissipation or transfer",
        "Negative feedback (stabilizing)",
        "Partial control (reduce, not eliminate)" ;
    ns1:discoveryContext "Nuclear reactor moderator (water, graphite) slowing fast neutrons revealed universal pattern of rate control in rapid processes." ;
    ns1:m2Basis "m2:Amplification (direction: attenuating)",
        "m2:Homeostasis",
        "m2:Regulation" ;
    ns1:primaryFunctions "Bandwidth limiting (filter high frequencies)",
        "Energy reduction (fast → slow)",
        "Rate control (limit reaction speed)",
        "Stabilization (prevent runaway)" ;
    ns1:tensorFormula "Regulation ⊗ Attenuation ⊗ Stabilization" ;
    ns1:transdisciplinaryExamples [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ] ;
    ns1:validationCount 7 .

ns1:PassiveSafety a owl:Class ;
    rdfs:label "Passive Safety" ;
    rdfs:comment "Safety mechanism requiring NO external power or human intervention. Relies on physics (gravity, natural circulation), material properties (melting points), or fail-safe design. Examples: HTR natural cooling, MSR freeze plug, automotive crumple zones, dead man's switch." ;
    skos:definition "Design philosophy where safe state is achieved through inherent physical principles (gravity, thermodynamics, material properties) rather than active systems requiring power or operator action. Fail-safe by default." ;
    skos:related "m1core:ModeratorMechanism",
        "m2:Constraint",
        "m2:Resilience" ;
    ns1:contrastWithActiveSafety [ ] ;
    ns1:designPrinciples "Fail-safe (safe state is default, requires no energy)",
        "Inherent property (not added feature)",
        "No external power needed",
        "No operator action required",
        "Physics-based (gravity, natural circulation, material limits)" ;
    ns1:discoveryContext "HTR and MSR passive safety features revealed universal design pattern of physics-based fail-safe mechanisms." ;
    ns1:m2Basis "m2:Constraint",
        "m2:Resilience",
        "m2:Structure" ;
    ns1:nuclearSafetyExample [ ] ;
    ns1:tensorFormula "Constraint ⊗ Resilience ⊗ InherentProperty" ;
    ns1:transdisciplinaryExamples [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ] ;
    ns1:validationCount 7 .

ns1:SelfSustainingReaction a owl:Class ;
    rdfs:label "Self-Sustaining Reaction" ;
    rdfs:comment "Process where products enable continuation of process (output → input positive feedback loop). Requires critical mass/threshold to initiate. Examples: fission chain reaction, combustion, autocatalysis, viral spread." ;
    skos:definition "Dynamic process characterized by positive feedback where reaction products trigger new reactions, creating potential for exponential growth if unregulated. Always exhibits threshold behavior (critical point)." ;
    skos:related "m1core:CriticalityRegime",
        "m1core:ModeratorMechanism",
        "m2:Amplification",
        "m2:Cycle" ;
    ns1:controlStrategies "Negative feedback (homeostasis)",
        "Product removal (interrupt cycle)",
        "Rate reduction (moderator mechanism)",
        "Threshold maintenance (critical point)" ;
    ns1:discoveryContext "Nuclear fission chain reaction analysis revealed universal pattern of output → input positive feedback with threshold behavior." ;
    ns1:keyCharacteristic "Output → Input (positive feedback creates self-reinforcing loop)" ;
    ns1:m2Basis "m2:Amplification",
        "m2:Cycle",
        "m2:Process",
        "m2:Threshold" ;
    ns1:requiredConditions "Amplification (gain ≥ 1 per cycle)",
        "Initial trigger (seed, spark, catalyst)",
        "Positive feedback mechanism (output → input)",
        "Threshold (critical point for self-sustaining)" ;
    ns1:tensorFormula "Process ⊗ Cycle ⊗ Amplification (gain ≥ 1)" ;
    ns1:transdisciplinaryExamples [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ] ;
    ns1:validationCount 6 .

