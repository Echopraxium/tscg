    rdfs:domain "m3:MathematicalCategory" ;
    rdfs:range "xsd:string" .

<https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M3_GenesisSpace.jsonld#composedWith> a owl:ObjectProperty ;
    rdfs:label "composed with" ;
    rdfs:comment "Morphism composition: if f: A→B and g: B→C, then f composedWith g yields g∘f: A→C." ;
    rdfs:domain "m3:Morphism" ;
