      "m1:layerPosition": {
        "above": "M0 (Poclets - concrete instantiations)",
        "below": "M2 (Metaconcepts - universal patterns)",
        "role": "Domain-specific reusable concepts bridging universal (M2) and particular (M0)"
      },
      
      "m1:architecturalPrinciple": {
