      "m1core:m2Basis": [
        "m2:Constraint",
        "m2:Resilience",
        "m2:Structure"
      ],
      "m1core:tensorFormula": "Constraint ⊗ Resilience ⊗ InherentProperty",
      "m1core:designPrinciples": [
        "Fail-safe (safe state is default, requires no energy)",
        "Physics-based (gravity, natural circulation, material limits)",
        "No operator action required",
        "No external power needed",
        "Inherent property (not added feature)"
      ],
      "m1core:transdisciplinaryExamples": [
        {
          "domain": "Nuclear Engineering (HTR)",
          "passiveMechanism": "Natural circulation cooling + TRISO fuel integrity",
          "activeAlternative": "Forced cooling pumps requiring power",
          "failureMode": "Loss of power → natural convection maintains cooling, TRISO withstands 1600°C",
          "physicalBasis": "Buoyancy (hot gas rises), ceramic fuel melting point (2800°C >> operational 950°C)"
        },
        {
          "domain": "Nuclear Engineering (MSR)",
          "passiveMechanism": "Freeze plug (solid salt plug melts at 500°C)",
          "activeAlternative": "Manual valve actuation",
