          "physicalBasis": "Gravity drainage, phase transition temperature"
        },
        {
          "domain": "Automotive Safety",
          "passiveMechanism": "Crumple zones (controlled deformation)",
          "activeAlternative": "Airbags (explosive deployment requiring sensors)",
          "failureMode": "Collision → material yields, absorbs kinetic energy",
          "physicalBasis": "Material plastic deformation, energy absorption"
        },
        {
          "domain": "Aviation",
          "passiveMechanism": "Ram Air Turbine (RAT, wind-driven emergency generator)",
          "activeAlternative": "APU (Auxiliary Power Unit, fuel-powered)",
          "failureMode": "Total power loss → RAT deploys, windmilling generates emergency power",
          "physicalBasis": "Airstream kinetic energy conversion"
        },
        {
          "domain": "Building Safety",
          "passiveMechanism": "Fire doors with fusible link (melts at 74°C)",
          "activeAlternative": "Powered sprinkler pumps",
          "failureMode": "Fire heat → link melts → door closes automatically (gravity/spring)",
          "physicalBasis": "Material melting point, gravity, spring tension"
        },
        {
          "domain": "Electronics",
          "passiveMechanism": "Fuse (overcurrent melting)",
          "activeAlternative": "Circuit breaker (electromechanical trip)",
          "failureMode": "Overcurrent → fuse element melts → circuit open",
          "physicalBasis": "Resistive heating (I²R), material melting point"
        },
        {
          "domain": "Mechanical",
          "passiveMechanism": "Dead man's switch (spring return to safe state)",
          "activeAlternative": "Emergency stop button (requires operator action)",
          "failureMode": "Operator incapacitation → spring returns lever → power off",
          "physicalBasis": "Spring potential energy, mechanical linkage"
        }
      ],
      "m1core:contrastWithActiveSafety": {
        "activeSafety": "Requires power, sensors, actuators, operator action",
        "passiveSafety": "Inherent, always present, independent of external systems",
        "reliability": "Passive > Active (fewer failure modes, no common-cause failures)",
        "complexity": "Passive < Active (simpler, physics-based)",
        "cost": "Passive may be higher upfront (materials, design) but lower long-term (no maintenance)"
      },
      "m1core:nuclearSafetyExample": {
        "HTR": "Walk-away safe - operator can leave, reactor shuts down passively",
        "mechanism": [
          "Strong negative temperature coefficient (reactivity drops as temp rises)",
          "TRISO fuel withstands 1600°C without failure (2800°C melting point)",
          "Natural circulation removes decay heat (no pumps needed)",
          "Graphite moderator absorbs decay heat (thermal mass)"
        ],
        "result": "No operator action, no external power → reactor safe indefinitely"
      },
      "m1core:validationCount": 7,
      "m1core:discoveryContext": "HTR and MSR passive safety features revealed universal design pattern of physics-based fail-safe mechanisms.",
      "skos:related": [
        "m2:Resilience",
