    {
      "@id": "m1core:CriticalityRegime",
      "@type": "owl:Class",
      "rdfs:label": "Criticality Regime",
      "rdfs:comment": "System state relative to self-sustaining threshold. Three regimes: subcritical (process dies), critical (self-sustaining equilibrium), supercritical (exponential growth). Universal pattern appearing when process products enable continuation of process.",
      "skos:definition": "Classification of system state based on relationship to critical threshold where process becomes self-sustaining (reproduction number = 1, multiplication factor k = 1, percolation probability p = p_c).",
      "m1core:m2Basis": [
        "m2:Threshold",
        "m2:Bifurcation",
        "m2:Homeostasis"
      ],
      "m1core:tensorFormula": "Threshold ⊗ SelfSustainingReaction ⊗ Amplification",
      "m1core:regimes": {
        "subcritical": {
          "condition": "λ < 1 (eigenvalue of propagation matrix)",
          "behavior": "Process dies out naturally",
          "examples": [
            "Nuclear: k_eff < 1 (reactor shutdown)",
            "Epidemiology: R₀ < 1 (disease dies out)",
            "Finance: r < 0 (investment decay)",
            "Social: Viral coefficient < 1 (content dies)"
          ]
        },
        "critical": {
          "condition": "λ = 1 (exact threshold)",
          "behavior": "Self-sustaining equilibrium (steady-state)",
          "examples": [
            "Nuclear: k_eff = 1.0 (steady power)",
            "Epidemiology: R₀ = 1 (endemic equilibrium)",
            "Finance: r = 0 (break-even)",
            "Percolation: p = p_c (spanning cluster)"
          ]
        },
        "supercritical": {
          "condition": "λ > 1 (above threshold)",
          "behavior": "Exponential growth (runaway if uncontrolled)",
          "examples": [
            "Nuclear: k_eff > 1 (power increase)",
            "Epidemiology: R₀ > 1 (epidemic)",
            "Finance: r > 0 (compound growth)",
            "Social: Viral coefficient > 1 (viral spread)"
          ]
        }
      },
      "m1core:mathematicalStructure": {
        "differential_equation": "dx/dt = (λ - 1) × x",
        "solution_subcritical": "x(t) = x₀ × exp[-(1-λ)t] → 0 as t → ∞",
        "solution_critical": "x(t) = x₀ (constant)",
        "solution_supercritical": "x(t) = x₀ × exp[(λ-1)t] → ∞ as t → ∞"
      },
      "m1core:transdisciplinaryDomains": [
        "Nuclear Engineering (fission criticality)",
        "Epidemiology (disease reproduction number R₀)",
        "Chemistry (autocatalytic reactions)",
        "Percolation Theory (bond/site probability)",
        "Economics (growth/decay rates)",
        "Social Networks (viral spread)",
        "Ecology (population dynamics)",
        "Phase Transitions (temperature vs T_critical)"
      ],
      "m1core:validationCount": 8,
      "m1core:discoveryContext": "Emerged from Nuclear Reactor Typology poclet analysis. Pattern recognized as universal across all self-sustaining processes with threshold behavior.",
      "skos:related": [
        "m1core:SelfSustainingReaction",
        "m2:Threshold",
        "m2:Bifurcation"
      ]
    },

