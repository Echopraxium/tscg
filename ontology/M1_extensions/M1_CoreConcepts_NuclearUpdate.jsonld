    {
      "@id": "m1core:ModeratorMechanism",
      "@type": "owl:Class",
      "rdfs:label": "Moderator Mechanism",
      "rdfs:comment": "Component or process that slows, controls, or stabilizes a rapid/intense process. Function: energy reduction, rate control, stabilization. Examples: neutron moderator (nuclear), enzyme inhibitor (biology), damper (mechanics), low-pass filter (electronics).",
      "skos:definition": "Regulatory element that reduces process intensity or rate without stopping it entirely. Acts as intermediate control between full activity and complete shutdown.",
      "m1core:m2Basis": [
        "m2:Regulation",
        "m2:Amplification (direction: attenuating)",
        "m2:Homeostasis"
      ],
      "m1core:tensorFormula": "Regulation ⊗ Attenuation ⊗ Stabilization",
      "m1core:primaryFunctions": [
        "Energy reduction (fast → slow)",
        "Rate control (limit reaction speed)",
        "Stabilization (prevent runaway)",
        "Bandwidth limiting (filter high frequencies)"
      ],
      "m1core:transdisciplinaryExamples": [
        {
          "domain": "Nuclear Engineering",
          "rapidProcess": "Fast neutrons (2 MeV kinetic energy)",
          "moderator": "Light water (H₂O), heavy water (D₂O), or graphite (C)",
          "effect": "Thermalization to 0.025 eV (thermal neutrons)",
          "mechanism": "Elastic scattering collisions transfer neutron energy to moderator nuclei"
        },
        {
          "domain": "Chemistry",
          "rapidProcess": "Chain reaction (uncontrolled)",
          "moderator": "Inhibitor, catalyst poison",
          "effect": "Rate reduction, controlled reaction",
          "mechanism": "Competitive binding, active site blocking"
        },
        {
          "domain": "Biology",
          "rapidProcess": "Enzyme cascade (signal amplification)",
          "moderator": "Allosteric inhibitor, phosphatase",
          "effect": "Negative feedback, signal attenuation",
          "mechanism": "Enzyme deactivation, product inhibition"
        },
        {
          "domain": "Mechanical Engineering",
          "rapidProcess": "High-speed motion, vibration",
          "moderator": "Damper, shock absorber, brake",
          "effect": "Velocity reduction, oscillation damping",
          "mechanism": "Friction, viscous resistance, energy dissipation"
        },
        {
          "domain": "Electronics",
          "rapidProcess": "High-frequency signal",
          "moderator": "Low-pass filter (RC, LC)",
          "effect": "Frequency cutoff, noise reduction",
          "mechanism": "Capacitive/inductive impedance increases with frequency"
        },
        {
          "domain": "Social Systems",
          "rapidProcess": "Extreme opinions, inflammatory content",
          "moderator": "Forum moderator, community guidelines",
          "effect": "Civility enforcement, tone reduction",
          "mechanism": "Content removal, warning, timeout"
        },
        {
          "domain": "Economics",
          "rapidProcess": "Inflation, market volatility",
          "moderator": "Central bank policy (interest rates)",
          "effect": "Rate stabilization, market cooling",
          "mechanism": "Money supply control, lending cost adjustment"
        }
      ],
      "m1core:designPrinciples": [
        "Partial control (reduce, not eliminate)",
        "Continuous action (not binary on/off)",
        "Negative feedback (stabilizing)",
        "Energy dissipation or transfer"
      ],
      "m1core:validationCount": 7,
      "m1core:discoveryContext": "Nuclear reactor moderator (water, graphite) slowing fast neutrons revealed universal pattern of rate control in rapid processes.",
      "skos:related": [
        "m2:Regulation",
        "m2:Amplification",
        "m1core:SelfSustainingReaction"
      ]
    },

    {
      "@id": "m1core:DualCircuitArchitecture",
      "@type": "owl:Class",
      "rdfs:label": "Dual Circuit Architecture",
      "rdfs:comment": "System design with separated primary and secondary circuits connected via interface/barrier. Function: isolation (safety, contamination), efficiency (heat exchange), or voltage transformation. Examples: PWR reactor (radioactive primary, clean secondary), HVAC (refrigerant/air), electrical transformer (high/low voltage).",
      "skos:definition": "Architectural pattern with two distinct circuits separated by barrier, enabling transfer (energy, information, material) while maintaining isolation (safety, pressure, voltage).",
      "m1core:m2Basis": [
        "m2:Structure",
        "m2:Interface",
        "m2:Flow"
      ],
      "m1core:tensorFormula": "Structure ⊗ Interface ⊗ Isolation",
      "m1core:architecturalComponents": {
        "primaryCircuit": "High-energy, high-risk, or contaminated side",
        "secondaryCircuit": "Low-energy, safe, or clean side",
        "interface": "Barrier enabling transfer while maintaining separation",
        "barrier_types": [
          "Physical (heat exchanger tubes, transformer coil insulation)",
          "Thermal (temperature gradient)",
          "Electrical (galvanic isolation)",
          "Chemical (membrane, filter)"
        ]
      },
      "m1core:transdisciplinaryExamples": [
        {
          "domain": "Nuclear Engineering (PWR)",
          "primaryCircuit": "Radioactive water (155 bar, 325°C, activated corrosion products)",
          "secondaryCircuit": "Clean steam (70 bar, 280°C, drives turbine)",
          "interface": "Steam generator (U-tubes, Inconel 600/690)",
          "purpose": "Radiation containment, prevent radioactive steam reaching turbine"
        },
        {
          "domain": "Thermal Engineering",
          "primaryCircuit": "Heat source fluid (hot side)",
          "secondaryCircuit": "Heat sink fluid (cold side)",
          "interface": "Heat exchanger (shell-and-tube, plate)",
          "purpose": "Temperature control, thermal isolation"
        },
        {
          "domain": "Hydraulic Systems",
          "primaryCircuit": "High-pressure oil (3000 psi)",
          "secondaryCircuit": "Low-pressure oil (500 psi)",
          "interface": "Pressure reducer valve",
          "purpose": "Safety, prevent overpressure in actuators"
        },
        {
          "domain": "Electrical Engineering",
          "primaryCircuit": "High voltage (11 kV distribution)",
          "secondaryCircuit": "Low voltage (220 V user)",
          "interface": "Transformer (magnetic coupling, no electrical connection)",
          "purpose": "User safety, voltage matching"
        },
        {
          "domain": "HVAC (Air Conditioning)",
          "primaryCircuit": "Refrigerant loop (R-410A, high pressure)",
          "secondaryCircuit": "Air loop (indoor circulation)",
          "interface": "Evaporator coil (cold side), condenser coil (hot side)",
          "purpose": "Thermal transfer, refrigerant containment"
        },
        {
          "domain": "Automotive",
          "primaryCircuit": "Engine coolant (ethylene glycol, 90-110°C)",
          "secondaryCircuit": "Cabin air (fresh or recirculated)",
          "interface": "Heater core (small radiator in dashboard)",
          "purpose": "Thermal comfort, coolant containment"
        }
      ],
      "m1core:keyProperties": [
        "Physical separation (two distinct circuits)",
        "Barrier interface (heat exchanger, transformer, membrane)",
        "Transfer mechanism (thermal, electrical, chemical)",
        "Isolation guarantee (safety, contamination prevention)"
      ],
      "m1core:designAdvantages": [
        "Safety (contamination/radiation contained in primary)",
        "Pressure decoupling (different operating pressures)",
        "Efficiency (optimize each circuit independently)",
        "Maintenance (secondary circuit accessible, primary sealed)"
      ],
      "m1core:validationCount": 6,
      "m1core:discoveryContext": "PWR nuclear reactor primary/secondary circuit separation revealed universal architectural pattern for safe energy transfer.",
      "skos:related": [
        "m2:Structure",
        "m2:Interface",
        "m2:Flow"
      ]
    },

    {
