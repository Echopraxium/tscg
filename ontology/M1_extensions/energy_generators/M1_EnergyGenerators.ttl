@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix m1energy: <https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M1_extensions/energy/M1_EnergyGenerators.jsonld#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

m1energy:EnergyGenerator a owl:Class ;
    rdfs:label "Energy Generator" ;
    rdfs:comment "Abstract base class for all power generation systems. Subclass of m2:Processor." ;
    rdfs:subClassOf "m2:Processor" .

m1energy:M1_EnergyGenerators a owl:Ontology ;
    dcterms:created "2026-02-10" ;
    dcterms:creator "Echopraxium with the collaboration of Claude AI" ;
    dcterms:description "Domain extension for energy generation systems. Nuclear reactors fully modeled (9 types), fossil/renewable planned." ;
    dcterms:title "TSCG M1 Energy Generators Extension" ;
    owl:imports "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M1_CoreConcepts.jsonld",
        "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M2_MetaConcepts.jsonld" ;
    owl:versionInfo "1.0.0" .

m1energy:NuclearReactor a owl:Class ;
    rdfs:label "Nuclear Reactor" ;
    rdfs:comment "Power generation via controlled nuclear fission. See M0_NuclearReactorTypology for 9 reactor types." ;
    rdfs:subClassOf "m1energy:EnergyGenerator" .

