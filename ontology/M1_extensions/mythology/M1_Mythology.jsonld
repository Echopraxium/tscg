    {
      "@id": "m1:mythology:MythologicalPattern",
      "@type": "owl:Class",
      "rdfs:label": "Mythological Pattern",
      "rdfs:comment": "Domain-specific pattern appearing in mythological/cosmological systems. Instantiates M2 metaconcepts (Structure, Flow, Dynamics, Polarity, etc.) in culturally-specific symbolic forms. Unlike M1_CoreConcepts (transdisciplinary), these patterns are bounded to mythology domain but transferable across cultures.",
      "rdfs:subClassOf": "m1:core:DomainPattern",
      
      "m1:domainBoundedness": "Mythology / Cosmology / Spiritual Systems",
      "m1:culturalTransferability": "High - patterns appear across cultures with local symbolic variations",
      "m1:symbolicNature": "Patterns operate through symbolic/metaphorical mechanisms rather than physical causation"
    },
    
    {
      "@id": "m1:mythology:CosmologicalAxis",
      "@type": [
        "owl:NamedIndividual",
        "m1:mythology:MythologicalPattern"
      ],
      "rdfs:label": "Cosmological Axis",
      "skos:altLabel": ["Axis Mundi", "World Axis", "Vertical Hierarchy"],
      
      "rdfs:comment": "Vertical hierarchical structure organizing cosmological realms from lowest (underworld/origins) through middle (manifest world) to highest (heaven/transcendence). Establishes ontological stratification and enables vertical movement/communication between realms. Embodied as world tree, cosmic mountain, pillar, or ladder.",
      
      "m1:m2Basis": [
        "m2:Hierarchy (S⊗A) - Vertical ordering with dominance relations",
        "m2:Structure (S) - Spatial organization along vertical axis",
        "m2:Space (S⊗I) - Dimensional framework for cosmological levels"
      ],
      
      "m1:tensorFormula": "S⊗A⊗I (Structure × Attractor × Information)",
      
      "m1:principle": "Cosmological realms are organized vertically with ontological significance: lower = primordial/material/unconscious, middle = manifest/experiential/conscious, upper = ideal/spiritual/superconscious. Vertical axis enables flow (souls, messages, energies, visions) between stratified ontological levels.",
      
      "m1:minimalStructure": {
        "minimumLevels": 3,
        "typicalLevels": "3-10",
        "archetypalTriad": "Underworld - Earth - Heaven",
        "embodiment": "Tree, mountain, pillar, ladder, rainbow bridge, cosmic serpent"
      },
      
      "m1:functionalProperties": {
        "ontologicalGradient": "Material density decreases, spiritual refinement increases with altitude",
        "accessibilityGradient": "Lower accessible to all; upper restricted (enlightenment, death, divine status)",
        "communicationChannels": "Vertical axis enables soul travel, divine descent, shamanic journeying, prayer ascent",
        "symbolicCorrespondences": "Roots=origins/unconscious, Trunk=present/conscious, Crown=destiny/superconscious"
      },
      
      "m1:examplesCrossCultural": [
        "Norse Yggdrasil: Niflheim (roots/underworld) - Midgard (trunk/earth) - Ásgard (branches/heaven)",
        "Kabbale Tree of Life: Malkuth (Kingdom/earth) - Tiferet (Beauty/heart) - Keter (Crown/divine) + 7 intermediate Sefirot",
        "Hindu/Buddhist Mount Meru: Patala (underworld) - Bhumi (earth) - Svarga (heaven realms)",
        "Greek: Tartarus (underworld prison) - Earth (Gaia) - Mount Olympus (divine abode)",
        "Chinese: Yellow Springs (underworld) - Middle Kingdom (earth) - Jade Emperor's Heaven",
        "Mesoamerican: Xibalba (9 underworld levels) - Earth - 13 celestial levels",
        "Shamanic: Lower World (animal spirits) - Middle World (ordinary reality) - Upper World (celestial guides)",
        "Christian: Hell - Earth - Heaven (with Purgatory as intermediate)",
        "Egyptian: Duat (underworld) - Earth - Sky goddess Nut"
      ],
      
      "m1:yggdrasilManifestation": "Vertical integration: Niflheim/Hel (roots, memory, origins) → Midgard (horizontal trunk, present) → Ásgard/Vanaheim (branches, divine order) → Tree Crown/Eagle (ultimate height, omniscient perspective). Bifröst rainbow bridge enables vertical travel.",
      
      "m1:psychologicalCorrespondence": "Jungian: Vertical axis = individuation process. Lower World = shadow/unconscious, Middle World = ego/persona, Upper World = Self/transcendent function. Shamanic journey = psychological integration.",
      
      "m1:validatedPoclets": 1,
      "m1:validatingPoclet": "Yggdrasil",
      "m1:transdisciplinaryPotential": "Medium - specific to cosmologies, but vertical hierarchy pattern generalizes to: organizational charts, biological taxonomy, Maslow's hierarchy, chakra systems",
      
      "m1:distinctionFromRelated": {
        "vs_Hierarchy_M2": "Hierarchy (M2) is generic vertical ordering; Cosmological Axis adds ontological/spiritual stratification and symbolic embodiment",
        "vs_MultipolarNetwork": "Multipolar Network is lateral/horizontal equivalence; Cosmological Axis is vertical dominance ordering"
      },
      
      "dcterms:created": "2026-01-22",
      "dcterms:source": "Yggdrasil poclet analysis"
    },
    
    {
      "@id": "m1:mythology:PrimordialOpposition",
      "@type": [
        "owl:NamedIndividual",
        "m1:mythology:MythologicalPattern"
      ],
      "rdfs:label": "Primordial Opposition",
      "skos:altLabel": ["Cosmogonic Duality", "Generative Opposition"],
      
      "rdfs:comment": "Fundamental generative duality of opposed elemental/metaphysical principles (hot-cold, light-dark, chaos-order, etc.) whose tension/interaction creates manifest cosmos. Unlike static polarity, primordial opposition is actively cosmogonic - reality emerges from the dynamic gap/interplay between poles. Often precedes creation of differentiated world.",
      
      "m1:m2Basis": [
        "m2:Polarity (S⊗I⊗A, N=2) - Dual opposition of principles",
        "m2:Emergence (I⊗S⊗D) - Cosmos arises from opposition",
        "m2:Dynamics (D) - Tension drives cosmogenesis"
      ],
      
      "m1:tensorFormula": "D⊗S⊗I (Dynamics × Structure × Information)",
      
      "m1:principle": "Cosmos created not from primordial unity but from tension between opposed principles. The 'gap' or interaction zone between opposites (often called void, chaos, abyss) is generative - manifest reality crystallizes from this dynamic opposition. Opposites mutually define, require, and generate each other.",
      
      "m1:archetypalOppositions": {
        "thermal": "Fire ↔ Ice (heat ↔ cold)",
        "luminous": "Light ↔ Darkness",
        "structural": "Order ↔ Chaos",
        "temporal": "Eternity ↔ Becoming",
        "spatial": "Expansion ↔ Contraction",
        "existential": "Being ↔ Non-Being",
        "ethical": "Good ↔ Evil",
        "metaphysical": "Spirit ↔ Matter"
      },
      
      "m1:generativeZone": {
        "name": "The Gap / Void / Abyss",
        "examples": "Ginnungagap (Norse), Chaos (Greek), Wuji (Chinese), Tehom (Hebrew)",
        "function": "Space 'between' opposites where cosmos crystallizes",
        "nature": "Neither pole but potential for both; pregnant void"
      },
      
      "m1:examplesCrossCultural": [
        "Norse Yggdrasil: Niflheim (primordial ice/cold/stasis) ↔ Muspellheim (primordial fire/heat/expansion) → Ginnungagap (gap) → Ymir (first giant) → cosmos",
        "Chinese: Yin (passive, dark, cold, feminine, earth) ↔ Yang (active, bright, hot, masculine, heaven) → Taiji (supreme ultimate) → 10,000 things",
        "Zoroastrian: Ahura Mazda (light, truth, good) ↔ Angra Mainyu (darkness, lie, evil) → perpetual cosmic struggle",
        "Egyptian: Ma'at (cosmic order, truth, justice) ↔ Isfet (chaos, falsehood, disorder) → pharaoh maintains balance",
        "Greek: Apollonian (order, reason, light, form) ↔ Dionysian (chaos, ecstasy, dark, formlessness) → Greek culture",
        "Kabbale: Chesed (mercy, expansion, giving) ↔ Gevurah (severity, contraction, judgment) → Tiferet (beauty, balance)",
        "Gnostic: Pleroma (fullness, light, spirit) ↔ Kenoma (emptiness, darkness, matter) → material world as fallen mixture",
        "Hindu: Purusha (consciousness, eternal, unchanging) ↔ Prakriti (matter, temporal, changing) → manifest universe",
        "Manichean: Light ↔ Darkness (absolute dualism, ongoing cosmic battle)"
      ],
      
      "m1:yggdrasilManifestation": "Niflheim (north pole: frozen rivers, primordial ice, Hvergelmir source) ↔ Muspellheim (south pole: fire realm, heat, Surtr the fire giant). Their meeting in Ginnungagap (yawning void) melts ice → water drops → Ymir (hermaphroditic giant) → from Ymir's body cosmos created.",
      
      "m1:dynamicPatterns": {
        "perpetualStruggle": "Zoroastrian, Manichean - opposites in eternal conflict",
        "dialecticalSynthesis": "Chinese Yin-Yang - opposites generate third term (Taiji)",
        "cyclicDominance": "Egyptian Ma'at-Isfet - periodic restoration of order",
        "eschatologicalResolution": "Some cosmologies: final victory of one pole (Ragnarök, Apocalypse)"
      },
      
      "m1:philosophicalImplications": {
        "ontology": "Being arises from tension, not from unity",
        "epistemology": "Knowledge emerges from dialectic, not from single perspective",
        "ethics": "Good defined by opposition to evil (co-dependent)",
        "cosmology": "Creation is perpetual process, not one-time event"
      },
      
      "m1:validatedPoclets": 1,
      "m1:validatingPoclet": "Yggdrasil (Niflheim-Muspellheim cosmogonic opposition)",
      "m1:transdisciplinaryPotential": "High - appears in: dialectical philosophy (Hegel), thermodynamics (entropy-negentropy), biology (anabolism-catabolism), economics (supply-demand), psychology (conscious-unconscious)",
      
      "m1:distinctionFromRelated": {
        "vs_Polarity_M2": "Polarity (M2) is static binary pattern; Primordial Opposition is actively generative (creates cosmos)",
        "vs_Duality": "Philosophical duality may be static categories; Primordial Opposition is dynamic and cosmogonic",
        "vs_CyclicTension": "Cyclic Tension maintains existing system; Primordial Opposition generates new cosmos"
      },
      
      "dcterms:created": "2026-01-22",
      "dcterms:source": "Yggdrasil poclet analysis"
    },
    
    {
      "@id": "m1:mythology:RootFeederCycle",
      "@type": [
        "owl:NamedIndividual",
        "m1:mythology:MythologicalPattern"
      ],
      "rdfs:label": "Root-Feeder Cycle",
      "skos:altLabel": ["Triple Root System", "Source-Sustenance Pattern"],
      
      "rdfs:comment": "Structural pattern where cosmological system draws sustenance from multiple distinct sources (wells, springs, primordial elements) via functional conduits (roots, rivers, pillars). Each source provides qualitatively different essential resource (memory, wisdom, fate, vitality). System viability requires access to all sources - loss of any source → systemic degradation.",
      
      "m1:m2Basis": [
        "m2:Structure (S) - Spatial organization of root conduits",
        "m2:Flow (F) - Resource extraction and circulation from sources",
        "m2:Storage (S⊗F) - Wells/springs as primordial reservoirs"
      ],
      
      "m1:tensorFormula": "S⊗F⊗I (Structure × Flow × Information)",
      
      "m1:principle": "Cosmological systems sustained by extracting resources (water, knowledge, energy, destiny) from multiple geographically/ontologically separated sources. Each source provides unique qualitative sustenance; system health depends on maintaining access to ALL sources. Roots/conduits are functional infrastructure enabling continuous resource extraction and circulation.",
      
      "m1:archetypalStructure": {
        "minimumSources": 3,
        "typicalNumber": "3-4 (triadic or quaternary foundation)",
        "sourceTypes": {
          "primordial": "Memory, origins, chaos (past)",
          "wisdom": "Knowledge, ancient learning, oracle (eternal)",
          "fate": "Destiny, future, prophecy (future)",
          "vitality": "Energy, life-force, power (present)"
        },
        "geometricSignificance": "3 = minimum stable polygon (triangle); 4 = cardinal directions"
      },
      
      "m1:functionalProperties": {
        "resourceDifferentiation": "Each source qualitatively unique (not just quantity)",
        "systemDependency": "System requires ALL sources for full function (not redundant)",
        "cyclicReplenishment": "Sources typically self-renewing (inexhaustible wells/springs)",
        "guardianship": "Sources often protected by entities (dragons, serpents, spirits, deities)",
        "symbolicMapping": "Sources correspond to psychological/spiritual faculties"
      },
      
      "m1:examplesCrossCultural": [
        "Norse Yggdrasil: 3 roots → 3 wells: (1) Hvergelmir (Niflheim: primordial chaos, memory, Nídhögg gnaws), (2) Mímisbrunnr (Jötunheim: wisdom, Odin sacrificed eye), (3) Urðarbrunnr (Ásgard: fate, Nornes water tree)",
        "Kabbale: Tree of Life draws from Ein Sof (infinite source) through 10 Sefirot (divine emanations/channels)",
        "Hindu Mount Meru: 4 sacred rivers flow from peak to 4 cardinal directions (Ganges, Indus, Oxus, Sita)",
        "Christian Eden: Tree of Life watered by 4 rivers (Pishon, Gihon, Tigris, Euphrates) from single source",
        "Chinese Kunlun Mountains: 4 celestial rivers originating from cosmic axis",
        "Celtic Bile (sacred tree): Rooted in 5 provinces, draws from otherworld sources (Tír na nÓg)",
        "Mesopotamian: Abzu (primordial freshwater abyss) feeds all rivers and springs",
        "Mayan World Tree: Roots in Xibalba (underworld), trunk on earth, branches in heaven - 3-level sustenance"
      ],
      
      "m1:yggdrasilManifestation": {
        "root1": {
          "destination": "Niflheim/Hvergelmir",
          "source": "Hvergelmir (boiling/roaring spring)",
          "quality": "Primordial memory, chaotic origins",
          "guardian": "Nídhögg (dragon perpetually gnaws root)",
          "symbolism": "Unconscious, past, material origins"
        },
        "root2": {
          "destination": "Jötunheim/Mímisbrunnr",
          "source": "Mímisbrunnr (Mímir's well)",
          "quality": "Ancient wisdom, cosmic knowledge",
          "guardian": "Mímir (decapitated head, oracle)",
          "symbolism": "Deep knowledge, timeless wisdom, Odin sacrificed eye here"
        },
        "root3": {
          "destination": "Ásgard/Urðarbrunnr",
          "source": "Urðarbrunnr (Well of Fate)",
          "quality": "Woven destiny, future, cosmic law",
          "guardian": "Nornes (Urd, Verdandi, Skuld - Fates)",
          "symbolism": "Destiny, divine order, Nornes draw water to restore tree"
        }
      },
      
      "m1:triplicitySignificance": "3 roots = minimum geometrically stable foundation (cf. Fire Triangle 3 components, tripod 3 legs). Echoes broader mythological preference for triadic structures: 3 Fates (Greek), 3 Graces, Holy Trinity (Christian), Trimurti (Hindu: Brahma-Vishnu-Shiva), 3 Worlds (shamanic).",
      
      "m1:validatedPoclets": 1,
      "m1:validatingPoclet": "Yggdrasil (3 roots → 3 distinct wells)",
      "m1:transdisciplinaryPotential": "Medium-High - generalizes to: three-legged stool (engineering stability), tripartite government (separation of powers), triple bottom line (business: profit-people-planet), three pillars of sustainability",
      
      "m1:distinctionFromRelated": {
        "vs_Network_M2": "Network is general connectivity; Root-Feeder emphasizes sustenance extraction infrastructure with source differentiation",
        "vs_Flow_M3": "Flow (M3) is generic transfer; Root-Feeder specifies multiple-source diversity requirement",
        "vs_Hierarchy_M2": "Hierarchy has vertical dominance; Root-Feeder has horizontal/lateral sustenance supply"
      },
      
      "dcterms:created": "2026-01-22",
      "dcterms:source": "Yggdrasil poclet analysis"
    },
    
    {
      "@id": "m1:mythology:PerpetualRestoration",
      "@type": [
        "owl:NamedIndividual",
        "m1:mythology:MythologicalPattern"
      ],
      "rdfs:label": "Perpetual Restoration",
      "skos:altLabel": ["Eternal Maintenance", "Cosmic Upkeep", "Divine Labor"],
      
      "rdfs:comment": "Dynamic pattern where cosmic system requires continuous active maintenance (watering, feeding, sacrifice, ritual) to counteract perpetual degradation forces. Cosmic order is NOT self-sustaining - it demands ongoing restorative labor by divine/semi-divine agents. Failure of restoration → systemic collapse (Ragnarök, Kali Yuga end, cosmic entropy). Often involves human ritual participation.",
      
      "m1:m2Basis": [
        "m2:Process (D⊗F) - Temporal evolution requiring intervention",
        "m2:Homeostasis (A⊗S⊗F) - Maintenance of stable cosmic state",
        "m2:Regulation (A⊗S⊗F) - Control mechanisms preventing collapse",
        "m1:core:CyclicTension (D⊗A⊗F) - Underlying degradation ↔ restoration opposition"
      ],
      
      "m1:tensorFormula": "D⊗A⊗F (Dynamics × Attractor × Flow)",
      
      "m1:principle": "Cosmic order is fragile, not eternal - it requires perpetual care. Degradation forces (entropy, chaos agents, time's erosion, evil) constantly threaten system integrity. Restoration agents (gods, spirits, priests, rituals) maintain viability through daily/seasonal/cyclical actions. Without restoration → cosmic degradation accelerates → eschatological collapse.",
      
      "m1:characteristicProperties": {
        "temporality": "Perpetual (no final resolution except eschatological)",
        "agency": "Requires active divine/semi-divine/priestly intervention",
        "actionType": "Ritual, cyclical (daily watering, seasonal sacrifice, yearly festival)",
        "consequence": "Failure → systemic degradation or apocalyptic collapse",
        "humanRole": "Often includes mandatory human participation (rituals sustaining cosmos)",
        "eschatology": "Perpetual restoration may eventually fail → end of cosmic cycle"
      },
      
      "m1:examplesCrossCultural": [
        "Norse Yggdrasil: Nornes (Urd, Verdandi, Skuld) water tree daily from Urðarbrunnr to counteract Nídhögg's gnawing. Without this → tree withers → Ragnarök",
        "Egyptian: Ra's nightly journey through Duat (underworld), battling Apophis (chaos serpent) to ensure sunrise. Priests perform rituals to aid Ra.",
        "Aztec: Blood sacrifice required to sustain sun's motion. Without human sacrifice → sun stops → cosmic stasis/death",
        "Hindu: Vishnu's periodic avatars (10 incarnations) to restore dharma when adharma (disorder) threatens cosmic order",
        "Greek: Horae (goddesses of seasons) maintain cosmic cycle. Vestal Virgins keep sacred fire of Rome (if extinguished → Rome falls)",
        "Babylonian: Annual Akitu festival re-enacts Marduk's defeat of Tiamat (chaos). Ritual sustains cosmic order for next year",
        "Chinese: Emperor performs rituals to sustain Mandate of Heaven. Neglect → natural disasters → dynastic collapse",
        "Mayan: Calendar rituals at cycle endings prevent cosmic collapse",
        "Jewish: Daily temple sacrifices maintain covenant (destroyed temple → diaspora)"
      ],
      
      "m1:yggdrasilManifestation": {
        "degradationAgent": "Nídhögg (dragon) perpetually gnaws at Yggdrasil roots (Hvergelmir)",
        "restorationAgents": "Nornes (3 Fates: Urd=Past, Verdandi=Present, Skuld=Future)",
        "restorationAction": "Draw water from Urðarbrunnr (Well of Fate) daily, sprinkle on tree roots",
        "consequence": "Tree remains viable, cosmos stable. Without restoration → tree withers → Ragnarök inevitable",
        "cyclicity": "Daily cycle (Nornes' labor is perpetual, not one-time)",
        "eschatology": "Ragnarök = moment when restoration can no longer match degradation"
      },
      
      "m1:ritualDimension": {
        "humanParticipation": "Mortals often required to participate in cosmic maintenance",
        "examples": "Aztec sacrifice, Egyptian temple rituals, Vedic fire ceremonies, Jewish temple service",
        "anthropologicalView": "Ritual as 'cosmological maintenance work' (Eliade)",
        "socialFunction": "Rituals bind community to cosmic order, give humans cosmic purpose"
      },
      
      "m1:philosophicalImplications": {
        "ontology": "Being is not self-sustaining but requires active maintenance",
        "cosmology": "Cosmos tends toward chaos (entropy), order requires work",
        "theology": "Gods are not omnipotent observers but active cosmic laborers",
        "anthropology": "Humans are co-creators/co-maintainers of cosmic order (not passive)",
        "eschatology": "Time is linear (toward eventual failure) or cyclical (perpetual renewal)"
      },
      
      "m1:relationToCyclicTension": "Perpetual Restoration is mythological instantiation of m1:core:CyclicTension. Degradation pole (Nídhögg, Apophis, entropy) opposed by restoration pole (Nornes, Ra, ritual). Both poles essential - without degradation, no need for divine labor; without restoration, cosmic collapse.",
      
      "m1:validatedPoclets": 1,
      "m1:validatingPoclet": "Yggdrasil (Nídhögg gnawing ↔ Nornes watering perpetual cycle)",
      "m1:transdisciplinaryPotential": "High - generalizes to: system maintenance (engineering), immune system (biology), monetary policy (economics), cultural preservation (sociology), ecosystem management (ecology)",
      
      "m1:distinctionFromRelated": {
        "vs_CyclicTension_M1core": "Cyclic Tension (M1 core) is generic degradation-restoration; Perpetual Restoration emphasizes divine agency, ritual, and cosmological necessity",
        "vs_Homeostasis_M2": "Homeostasis is automatic self-regulation; Perpetual Restoration requires active external intervention",
        "vs_Process_M2": "Process is generic temporal evolution; Perpetual Restoration is specific maintenance imperative with eschatological stakes"
      },
      
      "dcterms:created": "2026-01-22",
      "dcterms:source": "Yggdrasil poclet analysis"
    }
  ]
