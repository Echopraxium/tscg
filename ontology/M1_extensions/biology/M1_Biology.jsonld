  "@graph": [
    {
      "@id": "m1:biology:M1_Biology",
      "@type": "owl:Ontology",
      "rdfs:label": "TSCG M1 Biology Extension",
      "rdfs:comment": "Domain-specific biological concepts for TSCG framework. Contains patterns and concepts specific to biological systems (cellular, physiological, developmental).",
      "dcterms:title": "TSCG M1 Biology - Domain-Specific Biological Concepts",
      "dcterms:description": "Extension of TSCG framework with biology-specific concepts including cellular communication, signal transduction, homeostasis, and physiological regulation.",
      "dcterms:creator": "Echopraxium with the collaboration of Claude AI",
      "dcterms:created": "2026-01-22",
      "owl:versionInfo": "1.1.0",
      "m1:changelog": {
        "v1.1.0": {
          "date": "2026-02-18",
          "changes": [
            "UPDATE: m1:biology:FeedbackLoop now properly instantiates m2:FeedbackLoop (new M2 metaconcept v14.5.0)",
            "FORMULA: m2:Process ⊗ m2:Alignment ⊗ m2:Homeostasis (= A⊗S⊗F⊗I⊗D full ASFID coverage)",
            "ENRICHED: Added biologicalComponents (sensor/comparator/effector), typed examples (negative/positive)",
            "ARCHITECTURE: Clear separation - m1:biology:FeedbackLoop is domain instantiation; m3:AlignmentLoop is TSCG epistemological mechanism"
          ]
        }
      },
      "owl:imports": [
        "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M1_CoreConcepts.jsonld",
        "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M2_MetaConcepts.jsonld"
      ]
    },
    {
      "@id": "m1:biology:CellularCommunication",
      "@type": "owl:Class",
      "rdfs:label": "Cellular Communication",
      "rdfs:comment": "General pattern for intercellular signaling and information exchange. Foundation for all cell-cell communication modes.",
      "rdfs:subClassOf": "m1:core:CommunicationPattern",
      "m1:biology:m2Basis": [
        "m2:Signal",
        "m2:Channel",
        "m2:Information"
      ],
      "m1:biology:tensorFormula": "S⊗I⊗F",
      "m1:biology:domains": [
        "Endocrinology",
        "Neuroscience",
        "Immunology",
        "Developmental Biology"
      ]
    },
    {
      "@id": "m1:biology:AutocrineSignaling",
      "@type": "owl:Class",
      "rdfs:label": "Autocrine Signaling",
      "rdfs:comment": "Self-signaling pattern where a cell responds to signals it secretes itself. Used for feedback regulation and autonomous cell behavior.",
      "rdfs:subClassOf": "m1:biology:CellularCommunication",
      "m1:biology:spatialRange": "Intracellular to immediate cell surface (~1-10 µm)",
      "m1:biology:temporalRange": "Milliseconds to seconds",
      "m1:biology:mechanism": "Local concentration gradient around secreting cell",
      "m1:biology:examples": [
        "T-cell IL-2 autocrine loops",
        "Cancer cell growth factor self-stimulation",
        "Neurotransmitter autoreceptors"
      ]
    },
    {
      "@id": "m1:biology:ParacrineSignaling",
      "@type": "owl:Class",
      "rdfs:label": "Paracrine Signaling",
      "rdfs:comment": "Local neighborhood signaling pattern where signals diffuse through extracellular space to affect nearby cells. Coordinates tissue-level activity.",
      "rdfs:subClassOf": "m1:biology:CellularCommunication",
      "m1:biology:spatialRange": "Local tissue microenvironment (~10 µm - 1 mm)",
      "m1:biology:temporalRange": "Seconds to minutes",
      "m1:biology:mechanism": "Fick's law diffusion through extracellular matrix",
      "m1:biology:examples": [
        "Prostaglandins in inflammation",
        "Nitric oxide vasodilation",
        "Neurotransmitters at synapses",
        "Morphogen gradients in development"
      ]
    },
    {
      "@id": "m1:biology:NeuroendocrineSignaling",
      "@type": "owl:Class",
      "rdfs:label": "Neuroendocrine Signaling",
      "rdfs:comment": "Hybrid signaling pattern combining neural speed with endocrine range. Neurons secrete hormones into bloodstream for systemic distribution.",
      "rdfs:subClassOf": "m1:biology:CellularCommunication",
      "m1:biology:spatialRange": "Systemic (entire organism via bloodstream)",
      "m1:biology:temporalRange": "Seconds (neural trigger) → Minutes to hours (hormonal effect)",
      "m1:biology:mechanism": "Neural activation triggers hormone secretion into circulation",
      "m1:biology:examples": [
        "Hypothalamic-pituitary axis",
        "Adrenal medulla catecholamine release",
        "Oxytocin release in lactation",
        "ADH/Vasopressin water regulation"
      ]
    },
    {
      "@id": "m1:biology:EndocrineSignaling",
      "@type": "owl:Class",
      "rdfs:label": "Endocrine Signaling",
      "rdfs:comment": "Systemic signaling pattern where specialized glands secrete hormones into bloodstream for organism-wide coordination. Classic long-range hormonal communication.",
      "rdfs:subClassOf": "m1:biology:CellularCommunication",
      "m1:biology:spatialRange": "Systemic (entire organism)",
      "m1:biology:temporalRange": "Minutes to hours (hormone half-life dependent)",
      "m1:biology:mechanism": "Convective transport via cardiovascular system",
      "m1:biology:examples": [
        "Insulin glucose regulation",
        "Thyroid hormone metabolic control",
        "Growth hormone",
        "Sex steroids (estrogen, testosterone)",
        "Cortisol stress response"
      ]
    },
    {
      "@id": "m1:biology:Hormone",
      "@type": "owl:Class",
      "rdfs:label": "Hormone",
      "rdfs:comment": "Signaling molecule secreted by cells that travels through body fluids to target cells expressing specific receptors.",
      "m1:biology:m2Basis": [
        "m2:Signal",
        "m2:Agent",
        "m2:Mediator"
      ],
      "m1:biology:categories": {
        "peptide": "Insulin, Growth hormone, Oxytocin (hydrophilic)",
        "steroid": "Cortisol, Estrogen, Testosterone (lipophilic)",
        "amino-acid-derived": "Thyroxine, Adrenaline, Melatonin"
      }
    },
    {
      "@id": "m1:biology:Receptor",
      "@type": "owl:Class",
      "rdfs:label": "Receptor",
      "rdfs:comment": "Molecular structure that binds specific signaling molecules and transduces signal into cellular response.",
      "m1:biology:m2Basis": [
        "m2:Interface",
        "m2:Transducer",
        "m2:Switch"
      ],
      "m1:biology:types": {
        "membrane": "G-protein coupled receptors (GPCRs), Receptor tyrosine kinases (RTKs), Ion channels",
        "intracellular": "Nuclear receptors (steroid hormones), Cytoplasmic receptors"
      }
    },
    {
      "@id": "m1:biology:SignalTransduction",
      "@type": "owl:Class",
      "rdfs:label": "Signal Transduction",
      "rdfs:comment": "Process by which extracellular signal is converted into intracellular response through cascade of molecular interactions.",
      "m1:biology:m2Basis": [
        "m2:Process",
        "m2:Cascade",
        "m2:Amplification"
      ],
      "m1:biology:components": [
        "Ligand-receptor binding",
        "Second messenger systems (cAMP, Ca²⁺, IP₃)",
        "Protein kinase cascades",
        "Gene expression modulation"
      ]
    },
    {
      "@id": "m1:biology:Homeostasis",
      "@type": "owl:Class",
      "rdfs:label": "Homeostasis",
      "rdfs:comment": "Maintenance of stable internal conditions despite external fluctuations. Achieved through negative feedback regulation.",
      "m1:biology:m2Basis": [
        "m2:Balance",
        "m2:Regulation",
        "m2:Feedback",
        "m2:Attractor"
      ],
      "m1:biology:parameters": [
        "Blood glucose (~5 mM)",
        "Body temperature (37°C)",
        "Blood pH (7.35-7.45)",
        "Calcium concentration (~2.5 mM)",
        "Blood pressure (120/80 mmHg)"
      ]
    },
    {
      "@id": "m1:biology:FeedbackLoop",
      "@type": "owl:Class",
      "rdfs:label": "Biological Feedback Loop",
      "rdfs:comment": "Domain-specific instantiation of m2:FeedbackLoop in biological systems. A cyclic regulatory circuit where a physiological output (e.g. hormone concentration, blood pressure) is sensed, compared to a set point (Homeostasis), and corrected via an effector mechanism (Alignment→Process). Can be negative (stabilizing) or positive (amplifying).",
      "m1:biology:m2Basis": "m2:FeedbackLoop",
      "m1:biology:tensorFormula": "m2:Process ⊗ m2:Alignment ⊗ m2:Homeostasis",
      "m1:biology:feedback_polarity": {
        "values": ["Negative", "Positive", "Mixed"],
        "description": "Negative: stabilizing (insulin-glucose, thyroid hormone, RAAS). Positive: amplifying (oxytocin in labor, blood clotting cascade)."
      },
      "m1:biology:examples": {
        "negative": [
          "Insulin-glucose homeostasis",
          "Thyroid hormone regulation (HPT axis)",
          "RAAS blood pressure control",
          "Core body temperature regulation"
        ],
        "positive": [
          "Oxytocin positive feedback during labor",
          "Blood clotting cascade amplification",
          "Action potential depolarization (Na⁺ influx)"
        ]
      },
      "m1:biology:biologicalComponents": {
        "sensor": "Receptor or sensory organ detecting current state (e.g. JGA cells for blood pressure)",
        "comparator": "Integration center comparing state to set point (e.g. hypothalamus)",
        "effector": "Organ or cell type producing corrective response (e.g. adrenal gland)"
      }
    },
    {
      "@id": "m1:biology:DiffusionGradient",
      "@type": "owl:Class",
      "rdfs:label": "Diffusion Gradient",
      "rdfs:comment": "Spatial concentration pattern driving molecular movement from high to low concentration. Governed by Fick's laws.",
      "m1:biology:m2Basis": [
        "m2:Gradient",
        "m2:Flow",
        "m2:Field"
      ],
      "m1:biology:physics": {
        "fickFirstLaw": "J = -D∇C (flux proportional to gradient)",
        "fickSecondLaw": "∂C/∂t = D∇²C (concentration evolution)",
        "diffusionCoefficient": "D ~ 10⁻¹⁰ m²/s for proteins in extracellular matrix"
      }
    },
    {
      "@id": "m1:biology:Morphogen",
      "@type": "owl:Class",
      "rdfs:label": "Morphogen",
      "rdfs:comment": "Signaling molecule that forms concentration gradient and specifies cell fate in developing tissue. Position in gradient determines cell identity.",
      "rdfs:subClassOf": "m1:biology:Hormone",
      "m1:biology:m2Basis": [
        "m2:Gradient",
        "m2:Information",
        "m2:Pattern"
      ],
      "m1:biology:examples": [
        "Hedgehog (vertebrate neural tube patterning)",
        "BMP (bone morphogenetic protein)",
        "Wnt (embryonic axis formation)",
        "Retinoic acid (limb development)"
      ]
    },
    {
      "@id": "m1:biology:HypothalamusPituitaryAxis",
      "@type": "owl:Class",
      "rdfs:label": "Hypothalamus-Pituitary Axis",
      "rdfs:comment": "Master neuroendocrine structure coordinating hormone secretion. Hypothalamus integrates neural signals and controls pituitary hormone release.",
      "m1:biology:m2Basis": [
        "m2:Hierarchy",
        "m2:Hub",
        "m2:Control"
      ],
      "m1:biology:axes": [
        "HPA axis (stress): CRH → ACTH → Cortisol",
        "HPG axis (reproduction): GnRH → LH/FSH → Sex steroids",
        "HPT axis (metabolism): TRH → TSH → Thyroid hormones"
      ]
    },
    {
      "@id": "m1:biology:Cell",
      "@type": "owl:Class",
      "rdfs:label": "Cell",
      "rdfs:comment": "Fundamental unit of life. Bounded compartment capable of metabolism, growth, and reproduction.",
      "m1:biology:m2Basis": [
        "m2:Component",
        "m2:Agent",
        "m2:System"
      ]
    },
    {
      "@id": "m1:biology:Tissue",
      "@type": "owl:Class",
      "rdfs:label": "Tissue",
      "rdfs:comment": "Organized ensemble of cells performing coordinated function. Level between cell and organ in biological hierarchy.",
      "m1:biology:m2Basis": [
        "m2:Composition",
        "m2:Network",
        "m2:Layer"
      ]
    },
    {
      "@id": "m1:biology:Organ",
      "@type": "owl:Class",
      "rdfs:label": "Organ",
      "rdfs:comment": "Structural unit composed of multiple tissue types performing specific physiological function.",
      "m1:biology:m2Basis": [
        "m2:Modularity",
        "m2:Function",
        "m2:Hierarchy"
      ]
    },
    {
      "@id": "m1:biology:BloodCirculation",
      "@type": "owl:Class",
      "rdfs:label": "Blood Circulation",
      "rdfs:comment": "Cardiovascular transport system distributing nutrients, gases, hormones, and immune cells throughout organism.",
      "m1:biology:m2Basis": [
        "m2:Flow",
        "m2:Network",
        "m2:Transport"
      ],
      "m1:biology:parameters": {
        "cardiacOutput": "~5 L/min at rest",
        "circulationTime": "~60 seconds complete circuit",
        "pressure": "120/80 mmHg (systolic/diastolic)"
      }
    },
    {
      "@id": "m1:biology:Metabolism",
      "@type": "owl:Class",
      "rdfs:label": "Metabolism",
      "rdfs:comment": "Sum of all chemical reactions maintaining life. Includes catabolism (breakdown) and anabolism (synthesis).",
      "m1:biology:m2Basis": [
        "m2:Process",
        "m2:Flow",
        "m2:Transformation"
      ]
    },
    {
      "@id": "m1:biology:GeneExpression",
      "@type": "owl:Class",
      "rdfs:label": "Gene Expression",
      "rdfs:comment": "Process by which genetic information (DNA) is converted to functional product (protein). Central dogma: DNA → RNA → Protein.",
      "m1:biology:m2Basis": [
        "m2:Information",
        "m2:Code",
        "m2:Process"
      ]
    },
    {
      "@id": "m1:biology:Inflammation",
      "@type": "owl:Class",
      "rdfs:label": "Inflammation",
      "rdfs:comment": "Complex biological response to harmful stimuli (pathogens, damaged cells, irritants). Involves immune cell recruitment and tissue repair.",
      "m1:biology:m2Basis": [
        "m2:Response",
        "m2:Cascade",
        "m2:Emergence"
      ],
      "m1:biology:characteristics": [
        "Rubor (redness)",
        "Calor (heat)",
        "Tumor (swelling)",
        "Dolor (pain)",
        "Functio laesa (loss of function)"
      ]
    },
    {
      "@id": "m1:biology:Synapse",
      "@type": "owl:Class",
      "rdfs:label": "Synapse",
      "rdfs:comment": "Specialized junction between neurons enabling signal transmission. Can be chemical (neurotransmitter) or electrical (gap junction).",
      "m1:biology:m2Basis": [
        "m2:Interface",
        "m2:Channel",
        "m2:Signal"
      ]
    },
    {
      "@id": "m1:biology:ImmuneResponse",
      "@type": "owl:Class",
      "rdfs:label": "Immune Response",
      "rdfs:comment": "Coordinated reaction of immune system to foreign antigens. Involves innate (immediate) and adaptive (specific, memory) components.",
      "m1:biology:m2Basis": [
        "m2:Response",
        "m2:Recognition",
        "m2:Memory"
      ]
    }
  ]
}
