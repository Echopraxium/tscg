  "@graph": [
    {
      "@id": "m1:core:M1_CoreConcepts_Ontology",
      "@type": "owl:Ontology",
      "dcterms:title": "TSCG M1 Core Concepts Ontology",
      "dcterms:description": "Third layer of the TSCG framework. Defines reusable transdisciplinary concepts that instantiate M2 metaconcepts into domain-bounded but cross-applicable patterns. M1_CoreConcepts serves as base ontology extended by domain-specific M1 ontologies (M1_Optics, M1_Photography, M1_Philosophy, etc.).",
      "dcterms:creator": "Echopraxium with the collaboration of Claude AI",
      "dcterms:created": "2026-01-22",
      "dcterms:modified": "2026-01-22",
      "owl:versionInfo": "1.0.0",
      
      "m1:layerPosition": {
        "above": "M0 (Poclets - concrete instantiations)",
        "below": "M2 (Metaconcepts - universal patterns)",
        "role": "Domain-specific reusable concepts bridging universal (M2) and particular (M0)"
      },
      
      "m1:architecturalPrinciple": {
        "description": "M1 layer uses EXTENSION MECHANISM: M1_CoreConcepts (base) is extended by domain-specific M1 ontologies",
        "baseOntology": "M1_CoreConcepts.jsonld (this file) - Generic transdisciplinary concepts",
        "extensionOntologies": "M1_[Domain].jsonld files (specific to disciplines)",
        "importMechanism": "owl:imports from M1_CoreConcepts + domain-specific additions",
        "namespaceStrategy": "Each M1 extension has own namespace (m1optics:, m1photo:, m1phil:, etc.)"
      },
      
      "m1:extensionOntologies": {
