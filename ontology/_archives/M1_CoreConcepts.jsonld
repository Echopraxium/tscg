      "owl:imports": [
        "https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M2_MetaConcepts.jsonld"
      ],
      
      "rdfs:seeAlso": "https://github.com/Echopraxium/tscg/blob/main/ontology/docs/TSCG_M1_Architecture.md",
      
      "rdfs:comment": "M1_CoreConcepts is the BASE ontology for M1 layer. It contains generic transdisciplinary concepts applicable across multiple domains. Domain-specific M1 ontologies (M1_Optics, M1_Photography, M1_Philosophy, etc.) import M1_CoreConcepts and add specialized patterns. This architecture allows: (1) Sharing common concepts, (2) Domain specialization, (3) Modular extension, (4) Clear namespace separation.",
      
      "m1:developmentStrategy": {
        "phase1": "Populate M1_CoreConcepts with generic mechanisms (Oscillator, Filter, etc.)",
        "phase2": "Develop domain-specific M1 extensions as needed by poclets",
        "phase3": "Identify patterns that should be promoted from domain-specific to core",
        "phase4": "Refine boundaries between M1_Core and M1_Domain extensions"
      },
      
      "m1:conceptDiscoveryProcess": {
        "description": "How M1 concepts are discovered and validated",
        "steps": [
          "1. Analyze poclets (M0) to identify recurring domain-specific patterns",
          "2. Check if pattern already exists in M2 (universal) - if yes, instantiate directly",
          "3. Check if pattern already exists in M1_Core or M1_Domain - if yes, reuse",
          "4. If novel, create M1 concept with: M2 basis, domain scope, examples, validation",
          "5. After 3+ poclets use same concept, consider it validated",
          "6. If concept appears in 3+ different domains, consider promoting to M1_Core or even M2"
        ]
      },
      
      "m1:validationStatus": {
        "m1:conceptsDefined": 0,
        "m1:conceptsValidated": 0,
        "m1:domainExtensions": 2,
        "m1:totalConceptsAcrossExtensions": 18,
        "m1:note": "M1_CoreConcepts base is intentionally sparse. Concepts live in domain extensions."
      },
      
      "m1:changelog": {
        "v1.0.0": {
          "date": "2026-01-22",
          "changes": [
            "INITIAL RELEASE: M1_CoreConcepts base ontology",
            "Defined extension architecture for M1 layer",
            "Documented namespace strategy (m1optics:, m1photo:, m1phil:, etc.)",
            "Identified 4 core concept categories: Mechanisms, Templates, Relations, Processes",
            "Established concept discovery and validation process",
            "Created framework for domain-specific M1 ontologies",
            "Current extensions: M1_Optics (8 patterns), M1_Photography (10 patterns)",
            "Proposed extensions: M1_Philosophy, M1_Chemistry, M1_Biology, M1_Economics, M1_PoliticalScience",
            "Version 1.0.0 marks architectural foundation - content to be populated incrementally"
          ]
        }
      }
