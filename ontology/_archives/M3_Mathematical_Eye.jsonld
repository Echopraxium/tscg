    "dcterms": "http://purl.org/dc/terms/"
  },
  "@graph": [
    {
      "@id": "m3:MathematicalEye",
      "@type": "owl:Ontology",
      "dcterms:title": "TSCG M3 - Mathematical Eye (Eagle Eye)",
      "dcterms:description": "Mathematical / Quantitative perspective of the bicephalous architecture. The Eagle eye provides precise measurement, formalization, calculation and objective analysis. Focuses on HOW systems work (mechanisms, formulas, metrics).",
      "dcterms:creator": "TSCG Project - Michel Favre",
      "dcterms:created": "2026-01-17",
      "dcterms:modified": "2026-01-17",
      "owl:versionInfo": "1.0.0",
      "m3:metaphor": "Eagle - Bird of prey with exceptional visual acuity (can see small prey from kilometers away). Represents precision, distance, measurement.",
      "m3:role": "Quantification, Formalization, Measurement, Calculation",
      "m3:complementaryTo": {
        "@id": "m3:PhilosophicalEye",
        "rdfs:seeAlso": "https://github.com/Echopraxium/tscg/blob/main/ontology/M3_Philosophical_Eye.jsonld",
        "dcterms:documentation": {
          "@id": "https://github.com/Echopraxium/tscg/blob/main/ontology/M3_Philosophical_Eye.jsonld",
          "dcterms:format": "application/ld+json",
          "dcterms:type": "Ontology"
        }
      },
      "m3:characteristics": {
        "precision": "High - focuses on exact values, measurements, coefficients",
        "abstraction": "High - uses mathematical formalisms (tensors, vectors, equations)",
        "objectivity": "Maximum - aims for observer-independent quantification",
        "scope": "Analytical - decomposes systems into measurable components",
        "output": "Numbers, formulas, metrics, coefficients, equations"
      },
      "m3:capabilities": [
        {
          "capability": "Measurement",
          "description": "Quantify system properties with numerical precision",
          "examples": [
            "ASFID coefficients (A=0.70, S=0.85, F=0.90, I=0.95, D=0.40)",
            "Epistemic gap ΔΘ = ‖|Ω⟩ - |M⟩‖ ≈ 0.35",
            "RGB values (255, 128, 64)",
            "Temperature (298.15 K)",
            "Reaction rate constant k = 2.3 × 10⁻⁴ s⁻¹"
          ]
        },
        {
          "capability": "Formalization",
          "description": "Express concepts as mathematical structures",
          "examples": [
            "Tensor formulas: S⊗I⊗F, A⊗S⊗I, D⊗I⊗S",
            "State vectors: |Ω⟩ = 0.70|A⟩ + 0.85|S⟩ + ...",
            "Transformation matrices: RGB ↔ HSL conversion",
            "Differential equations: dN/dt = rN(1 - N/K)"
          ]
        },
        {
          "capability": "Calculation",
          "description": "Compute derived quantities from measurements",
          "examples": [
            "Gap norm: √(Σᵢ(Ωᵢ - Mᵢ)²)",
            "Color distance: ΔE = √((L₁-L₂)² + (a₁-a₂)² + (b₁-b₂)²)",
            "Shannon capacity: C = B log₂(1 + SNR)",
            "Gibbs free energy: ΔG = ΔH - TΔS"
          ]
        },
        {
          "capability": "Comparison",
          "description": "Quantitative comparison between systems",
          "examples": [
            "ΔΘ_RGB = 0.35 vs ΔΘ_HSL = 0.37 (similar quality)",
            "Gamut size: RGB > CMYK",
            "Information content: CMYK (32-bit) > RGB (24-bit)",
            "Efficiency: K channel reduces ink cost by 40%"
          ]
        },
        {
          "capability": "Validation",
          "description": "Verify system properties through calculation",
          "examples": [
            "Energy conservation: E_in = E_out + E_dissipated",
            "Mass conservation: Reactants mass = Products mass",
            "RGB cube vertices: 2³ = 8 colors",
            "Total ink coverage: TAC ≤ 400%"
          ]
        }
      ],
      "m3:methodologies": [
        {
          "methodology": "ASFID State Quantification",
          "description": "Measure system state across 5 dimensions",
          "procedure": [
            "1. Identify observables for each dimension (A, S, F, I, D)",
            "2. Assign numerical coefficients [0.0, 1.0]",
            "3. Construct state vector |Ω⟩ or |M⟩",
            "4. Calculate norms and distances"
          ],
          "output": "ASFID signature: (A, S, F, I, D)"
        },
        {
          "methodology": "Epistemic Gap Calculation",
          "description": "Quantify Map-Territory divergence",
          "procedure": [
            "1. Measure Territory state: |Ω⟩",
            "2. Measure Map state: |M⟩",
            "3. Calculate difference: Δ = |Ω⟩ - |M⟩",
            "4. Compute norm: ΔΘ = ‖Δ‖",
            "5. Interpret gap magnitude"
          ],
          "output": "Gap metric ΔΘ ∈ [0, ∞)"
        },
        {
          "methodology": "Tensor Formula Assignment",
          "description": "Map metaconcepts to tensor products",
          "procedure": [
            "1. Identify dominant M3 dimensions",
            "2. Construct tensor product (e.g., S⊗I⊗F)",
            "3. Validate formula against examples",
            "4. Document formula in metaconcept"
          ],
          "output": "Tensor formula (e.g., A⊗S⊗I for Synergy)"
        },
        {
          "methodology": "Transformation Specification",
          "description": "Define mathematical transformations between representations",
          "procedure": [
            "1. Identify source and target spaces",
            "2. Specify transformation function f: X → Y",
            "3. Determine properties (bijective, continuous, etc.)",
            "4. Provide algorithm or formula"
          ],
          "output": "Transformation specification + algorithm"
        }
      ],
      "m3:tools": [
        "Linear algebra (vectors, matrices, tensors)",
        "Calculus (derivatives, integrals, differential equations)",
        "Statistics (mean, variance, correlation, distributions)",
        "Information theory (entropy, mutual information, channel capacity)",
        "Graph theory (networks, paths, connectivity)",
        "Set theory (unions, intersections, cardinality)",
        "Topology (continuity, compactness, connectedness)",
        "Geometry (distances, angles, symmetries)"
      ],
      "m3:strengths": [
        "Precision and exactness (no ambiguity)",
        "Reproducibility (same input → same output)",
        "Scalability (works for large/complex systems)",
        "Interoperability (mathematical language universal)",
        "Automation (computable by machines)"
      ],
      "m3:limitations": [
        "Loses qualitative richness (numbers don't convey meaning)",
        "Requires measurable observables (not all concepts quantifiable)",
        "Observer-dependent measurement (what to measure?)",
        "Context-insensitive (formula doesn't know application domain)",
        "Cannot answer WHY questions (only HOW and HOW MUCH)"
      ],
      "m3:examplesInTSCG": [
        {
          "context": "Fire Triangle Territory",
          "eagleView": "ASFID state = (0.85, 0.70, 0.90, 0.65, 0.75)",
          "calculation": "High flow (F=0.90) due to O₂ flux + heat dissipation",
          "output": "Quantitative characterization of combustion system"
        },
        {
          "context": "RGB Color Space",
          "eagleView": "RGB(255, 128, 0) = Orange",
          "calculation": "Hue H = arctan2(√3(G-B), 2R-G-B) ≈ 30°",
          "output": "Precise color specification"
        },
        {
          "context": "Map-Territory Gap",
          "eagleView": "ΔΘ_RGB = ‖(0.70,0.85,0.90,0.95,0.40) - (0.80,0.95,0.60,0.90,0.30)‖",
          "calculation": "ΔΘ = √(0.01+0.01+0.09+0.0025+0.01) ≈ 0.35",
          "output": "Quantified epistemic gap"
        },
        {
          "context": "CMYK Ink Savings",
          "eagleView": "GCR: K = min(C,M,Y); Total_ink_before = C+M+Y; Total_ink_after = (C-K)+(M-K)+(Y-K)+K = C+M+Y-2K",
          "calculation": "Savings = 2K / (C+M+Y) × 100%",
          "output": "Quantified cost reduction (e.g., 40%)"
        }
      ],
      "m3:bicephalousIntegration": {
        "description": "Mathematical Eye provides QUANTIFICATION; Philosophical Eye provides INTERPRETATION",
        "synergy": "Together, they create depth perception (epistemic gap as distance)",
        "workflow": [
          "1. Eagle measures (ASFID coefficients, gap ΔΘ)",
          "2. Lion interprets (What does ΔΘ=0.35 mean? Good model? Limitations?)",
          "3. Binocular synthesis (Quantified understanding)"
        ],
        "analogy": "Eagle sees HOW MUCH (measurements); Lion sees WHY and WHAT FOR (meaning, purpose, context)"
      },
      "m3:relatedConcepts": [
        {
          "concept": "Positivism",
          "relation": "Eagle aligns with positivist emphasis on measurement and empirical validation"
        },
        {
          "concept": "Reductionism",
          "relation": "Eagle decomposes systems into measurable components"
        },
        {
          "concept": "Pythagorean tradition",
          "relation": "'All is number' - mathematical structures as foundation of reality"
        },
        {
          "concept": "Information theory (Shannon)",
          "relation": "Quantification of information, channel capacity, entropy"
        }
      ],
      "rdfs:seeAlso": [
        {
          "@id": "m3:PhilosophicalEye",
          "rdfs:seeAlso": "https://github.com/Echopraxium/tscg/blob/main/ontology/M3_Philosophical_Eye.jsonld",
          "dcterms:documentation": {
            "@id": "https://github.com/Echopraxium/tscg/blob/main/ontology/M3_Philosophical_Eye.jsonld",
            "dcterms:format": "application/ld+json",
            "dcterms:type": "Ontology"
          }
        },
        {
          "@id": "m3:GenesisSpace",
          "rdfs:seeAlso": "https://github.com/Echopraxium/tscg/blob/main/ontology/M3_Genesis_Space.jsonld",
          "dcterms:documentation": {
            "@id": "https://github.com/Echopraxium/tscg/blob/main/ontology/M3_Genesis_Space.jsonld",
            "dcterms:format": "application/ld+json",
            "dcterms:type": "Ontology"
          }
        }
      ],
      "dcterms:references": [
        "Galileo (1623). The Assayer - 'Mathematics is the language of nature'",
        "Newton (1687). Principia Mathematica - Mathematical physics",
        "Shannon (1948). A Mathematical Theory of Communication",
        "Hilbert (1900). Mathematical Problems - Formalization program"
      ]
    }
  ]
