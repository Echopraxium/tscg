{
  "@context": {
    "m3": "https://github.com/Echopraxium/tscg/blob/main/ontology/M3_Mathematical_Eye.jsonld#",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
