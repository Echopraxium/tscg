      "@id": "m2:TSCG_M2_MetaConcepts_Ontology",
      "@type": "owl:Ontology",
      "dcterms:title": "TSCG M2 Metaconcepts Ontology",
      "dcterms:description": "Second layer of the TSCG (Transdisciplinary System Construction Game) framework. Defines 48 systemic patterns emerging from tensor products of M3 dimensions.",
      "dcterms:creator": "TSCG Project",
      "dcterms:created": "2026-01-14",
      "dcterms:modified": "2026-01-15",
      "owl:versionInfo": "7.1.0",
      "owl:imports": "https://github.com/Echopraxium/tscg/blob/main/ontology/TSCG_M3_Genesis_Ontology.jsonld",
      "rdfs:seeAlso": "https://github.com/Echopraxium/tscg/blob/main/ontology/docs/TSCG_M2_MetaConcepts_Ontology_README.md",
      "dcterms:documentation": {
        "@id": "https://github.com/Echopraxium/tscg/blob/main/ontology/docs/TSCG_M2_MetaConcepts_Ontology_README.md",
        "dcterms:format": "text/markdown",
        "dcterms:type": "Documentation"
      },
      "rdfs:comment": "M2 layer constructs metaconcepts through tensorization of M3 dimensions (A,S,F,I,D). Each metaconcept is characterized by its tensor formula, category, triple axes (Ontological Nature, Operational Direction, Granularity Direction), epistemic gap, and polarity.",
      "m2:tensorSpace": "T_M2 = ⊕_{k=1}^3 (H_M3)^⊗k",
      "m2:totalComponents": 155,
      "m2:componentBreakdown": {
        "order1": 5,
        "order2": 25,
        "order3": 125
      },
      "m2:tripleAxes": {
        "@type": "m2:TripleAxesFramework",
        "m2:axis1": {
          "m2:name": "Ontological Nature",
          "m2:values": ["ModelSpace", "RealitySpace", "DualNature"],
          "m2:description": "Distinguishes abstract representations (models) from tangible processes (reality)"
        },
        "m2:axis2": {
          "m2:name": "Operational Direction",
          "m2:values": ["Analytical", "Constructive", "DualProjection"],
          "m2:description": "Distinguishes observation/decomposition (analysis) from generation/synthesis (construction)"
        },
        "m2:axis3": {
          "m2:name": "Granularity Direction",
          "m2:values": ["Composition", "Decomposition", "Bidirectional", "N/A"],
          "m2:description": "Distinguishes aggregation (atoms→molecule, bottom-up) from disaggregation (molecule→atoms, top-down)",
          "m2:orthogonalTo": ["axis1", "axis2"],
          "m2:examples": {
            "composition": "LEGO bricks → construction, atoms → molecule, letters → word",
            "decomposition": "Construction → LEGO bricks, molecule → atoms, word → letters"
          }
        }
      },
      "m2:epistemicGapFormula": "ΔΘ(M) = ‖P(Reality) - Model‖ ∈ [0,1]",
      "m2:polarityPrinciple": "Binary only: neutral (unique) or dual (intrinsically bidirectional). N-ary polarity rejected.",
      "m2:categoryTheory": {
        "m2:functor_tensorization": "F_⊗: Cat_M3 → Cat_M2",
        "m2:functor_instantiation": "F_instantiate: Cat_M2 → Cat_M1",
        "m2:morphism_inclusion": "⊆ (e.g., Homeostasis ⊆ Regulation)",
        "m2:morphism_composition": "∘ (e.g., Learning = Memory ∘ Adaptation)",
        "m2:morphism_duality": "op (e.g., Divergence = Convergence^op)",
        "m2:morphism_emergence": "⇑ (multiple → emergent)"
      },
      "m2:criticalDecisions": {
        "m2:time_external": "Time is parameter in M3, not 6th dimension. Temporal metaconcepts (Memory, Process) require integration → M2",
        "m2:memory_is_m2": "M(t) = ∫(D-F)dτ requires temporal integration, hence M2 not M3",
        "m2:regulation_is_m2": "A⊗S⊗F coupling required, hence M2 not M3",
        "m2:flow_is_m3": "F is 3rd dimension with axiom F≥0.1, not M2 metaconcept",
        "m2:triple_axes_orthogonal": "Model/Reality ⊥ Analytical/Constructive ⊥ Composition/Decomposition",
        "m2:binary_dual_polarity_only": "Rejected n-ary (ternary, quaternary) as over-generalization",
        "m2:dual_polarity_added": "Dual polarity captures intrinsically bidirectional metaconcepts (e.g., Coding = Encoding/Decoding)",
        "m2:purity_principle": "M2 uses pure vocabulary (Ontology, Systems Theory, Cybernetics) - no domain-specific terms (biology, sociology, economics). Domain concepts belong to M1 extensions."
      },
      "m2:mathematicalValidationStatus": {
        "m2:status": "preliminary",
        "m2:validatedBy": "Claude (Anthropic LLM)",
        "m2:validationDate": "2026-01-15",
        "m2:requiresExpertReview": true,
        "m2:reviewAreas": [
          "Tensor product correctness",
          "Dirac notation usage",
          "Category theory morphisms",
          "Dimensional coherence",
          "Third-party validation (human experts, other LLMs)"
        ]
      },
      "m2:progress": {
        "m2:metaconceptsDefined": 48,
        "m2:metaconceptsTotal": 50,
        "m2:completionPercentage": 96,
        "m2:neutralPolarity": 41,
        "m2:dualPolarity": 7
      },
      "m2:changelog": {
        "v7.1.0": {
          "date": "2026-01-15",
          "changes": [
            "Network decomposition complete: 48 metaconcepts total",
            "Added Node (Structural, dual) - elementary network entity",
            "Added Relation (Relational, analytical) - abstract connection",
            "Added Path (Structural, dual) - sequence through network",
            "Added Cluster (Structural, constructive) - active processing group",
            "Added Hub (Structural, dual) - passive relay point",
            "Modified Link: DualNature→RealitySpace, DualProjection→Constructive, added Flow to formula (S⊗I→S⊗I⊗F)",
            "Structural category: 8→12 metaconcepts",
            "Relational category: 4→5 metaconcepts",
            "Completion: 96% (48/50)"
          ]
        },
        "v7.0.0": {
          "date": "2026-01-15",
          "changes": [
            "Complete ontology with 43 metaconcepts",
            "Added 27 new metaconcepts from validated list and candidates",
            "Added Storage, Capacity (replace Resource)",
            "Added Agent, Role, Mediator, Link (Relational category)",
            "Added Space (State Space, Phase Space)",
            "Added Tropism (dual: attraction/repulsion)",
            "Added Convergence/Divergence (dual)",
            "Added Signal (Informational: information carrier)",
            "Added comprehensive README documentation",
            "Updated triple axes framework with Axis 3 (Granularity Direction)"
          ]
        }
      }
    }
