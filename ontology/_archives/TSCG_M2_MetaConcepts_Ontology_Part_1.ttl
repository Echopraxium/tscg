m2:TSCG_M2_MetaConcepts_Ontology a owl:Ontology ;
    dcterms:created "2026-01-14" ;
    dcterms:creator "TSCG Project" ;
    dcterms:description "Second layer of the TSCG (Transdisciplinary System Construction Game) framework. Defines systemic patterns emerging from tensor products of M3 dimensions." ;
    dcterms:modified "2026-01-15" ;
    dcterms:title "TSCG M2 Metaconcepts Ontology" ;
    rdfs:comment "M2 layer constructs metaconcepts through tensorization of M3 dimensions (A,S,F,I,D). Each metaconcept is characterized by its tensor formula, category, triple axes (Model/Reality, Analytical/Constructive, Composition/Decomposition), epistemic gap, and polarity." ;
    owl:imports "https://github.com/Echopraxium/tscg/blob/main/ontology/TSCG_M3_Genesis_Ontology.jsonld" ;
    owl:versionInfo "6.5.0" ;
    m2:categoryTheory [ m2:functor_instantiation "F_instantiate: Cat_M2 → Cat_M1" ;
            m2:functor_tensorization "F_⊗: Cat_M3 → Cat_M2" ;
            m2:morphism_composition "∘ (e.g., Learning = Memory ∘ Adaptation)" ;
            m2:morphism_duality "op (e.g., Coherence^op = Entropy)" ;
            m2:morphism_emergence "⇑ (multiple → emergent)" ;
            m2:morphism_inclusion "⊆ (e.g., Homeostasis ⊆ Regulation)" ] ;
    m2:changelog [ ] ;
    m2:componentBreakdown [ ] ;
    m2:criticalDecisions [ m2:binary_polarity_only "Rejected n-ary (ternary, quaternary) as over-generalization" ;
            m2:dual_polarity_added "Dual polarity captures intrinsically bidirectional metaconcepts (e.g., Coding = Encoding/Decoding)" ;
            m2:flow_is_m3 "F is 3rd dimension with axiom F≥0.1, not M2 metaconcept" ;
            m2:memory_is_m2 "M(t) = ∫(D-F)dτ requires temporal integration, hence M2 not M3" ;
            m2:purity_principle "M2 must use pure vocabulary (Ontology, Systems Theory, Cybernetics) - no domain-specific terms (biology, sociology, economics). Domain concepts belong to M1 extensions." ;
            m2:regulation_is_m2 "A⊗S⊗F coupling required, hence M2 not M3" ;
            m2:time_external "Time is parameter in M3, not 6th dimension. Temporal metaconcepts (Memory, Trajectory) require integration → M2" ;
            m2:triple_axes_orthogonal "Model/Reality ⊥ Analytical/Constructive ⊥ Composition/Decomposition" ] ;
    m2:epistemicGapFormula "ΔΘ(M) = ‖P(Reality) - Model‖ ∈ [0,1]" ;
    m2:mathematicalValidationStatus [ m2:requiresExpertReview true ;
            m2:reviewAreas "Category theory morphisms",
                "Dimensional coherence",
                "Dirac notation usage",
                "Tensor product correctness",
                "Third-party validation (human experts, other LLMs)" ;
            m2:status "preliminary" ;
            m2:validatedBy "Claude (Anthropic LLM)" ;
            m2:validationDate "2026-01-15" ] ;
    m2:polarityPrinciple "Binary only: positive/negative pairs, dual (intrinsic bidirectionality), or neutral. N-ary polarity rejected." ;
    m2:progress [ m2:completionPercentage 68 ;
            m2:dualPolarity 6 ;
            m2:metaconceptsDefined 34 ;
            m2:metaconceptsTotal 50 ;
            m2:neutralPolarity 28 ] ;
    m2:tensorSpace "T_M2 = ⊕_{k=1}^3 (H_M3)^⊗k" ;
    m2:totalComponents 155 ;
    m2:tripleAxes [ a m2:TripleAxesFramework ;
            m2:axis1 [ m2:description "Distinguishes abstract representations (models) from tangible processes (reality)" ;
                    m2:name "Ontological Nature" ;
                    m2:values "DualNature",
                        "ModelSpace",
                        "RealitySpace" ] ;
            m2:axis2 [ m2:description "Distinguishes observation/decomposition (analysis) from generation/synthesis (construction)" ;
                    m2:name "Operational Direction" ;
                    m2:values "Analytical",
                        "Constructive",
                        "DualProjection" ] ;
            m2:axis3 [ m2:description "Distinguishes aggregation (atoms→molecule, bottom-up) from disaggregation (molecule→atoms, top-down)" ;
                    m2:examples [ ] ;
                    m2:name "Granularity Direction" ;
                    m2:orthogonalTo "axis1",
                        "axis2" ;
                    m2:values "Bidirectional",
                        "Composition",
                        "Decomposition",
                        "N/A" ] ] .

