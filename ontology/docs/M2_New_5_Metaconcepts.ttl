@prefix m2: <https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M2_MetaConcepts_Ontology.jsonld#> .
@prefix ns1: <rdfs:> .
@prefix ns2: <dcterms:> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

m2:Action a m2:MetaConcept,
        <owl:NamedIndividual> ;
    ns2:created "2026-01-28" ;
    ns2:creator "Echopraxium with the collaboration of Claude AI" ;
    m2:atomicity [ m2:granularity "Elementary operational unit" ;
            ns1:comment "Action is atomic - not further decomposable in behavioral context" ] ;
    m2:discoveryContext "Identified as atomic operational unit in behavioral decomposition. Represents finest-grained executable element triggered by Steps in Behavior/Workflow networks." ;
    m2:distinctionFromRelated [ ] ;
    m2:eagleView [ ] ;
    m2:hasCategory "m2:Dynamic" ;
    m2:hasDominantM3 "m3:eagle_eye:Dynamics",
        "m3:eagle_eye:Information" ;
    m2:hasEpistemicGap 1.5e-01 ;
    m2:hasExample "Baroreceptor activation (triggered by sense step)",
        "Cell division (triggered by egg stage step)",
        "Gradient descent update (triggered by training step)",
        "HTTP tokenize (triggered by parse step)",
        "Heat butter (triggered by make roux step)" ;
    m2:hasPolarity "dual" ;
    m2:hasTensorFormula "D⊗I" ;
    m2:perspective "dual" ;
    m2:polarityDescription [ ] ;
    m2:sphinxView [ ] ;
    m2:triggeringProperty [ m2:relationProperty "m2:triggeredBy" ;
            m2:triggerSource "m2:Step" ;
            ns1:comment "Action is triggered by Step" ] ;
    ns1:comment "Atomic operation triggered by a Step. Elementary executable unit with informational content. Represents indivisible operation in behavioral or workflow context." ;
    ns1:label "Action" .

m2:Behavior a m2:MetaConcept,
        <owl:NamedIndividual> ;
    ns2:created "2026-01-28" ;
    ns2:creator "Echopraxium with the collaboration of Claude AI" ;
    m2:decompositionProperty [ m2:cardinalityMin 1 ;
            m2:componentType "m2:Step" ;
            m2:relationProperty "m2:decomposedInto" ;
            ns1:comment "Behavior decomposes into Steps" ] ;
    m2:discoveryContext "Emerged from need to model sequential yet decomposable patterns in poclets (Butterfly metamorphosis, Blood pressure control). Bridges Process (pure temporal) and Network (pure structural) by fusing both aspects." ;
    m2:distinctionFromRelated [ ] ;
    m2:eagleView [ ] ;
    m2:hasCategory "m2:Dynamic" ;
    m2:hasDominantM3 "m3:eagle_eye:Dynamics",
        "m3:eagle_eye:Flow",
        "m3:eagle_eye:Structure" ;
    m2:hasEpistemicGap 2e-01 ;
    m2:hasExample "HTTP request handling (receive-parse-process-respond pattern)",
        "Learning algorithm (initialize-iterate-converge pattern)",
        "Photosynthesis (light-water-CO2 coordination pattern)",
        "Predator hunting sequence (stalk-chase-capture pattern)" ;
    m2:hasPolarity "dual" ;
    m2:hasTensorFormula "S⊗D⊗F" ;
    m2:perspective "dual" ;
    m2:polarityDescription [ ] ;
    m2:sphinxView [ ] ;
    ns1:comment "Network of coordinated Steps that can be decomposed and analyzed. Represents structured operational pattern observable in Territory and modelable in Map. Distinct from Process (pure temporal sequence) by adding structural decomposability." ;
    ns1:label "Behavior" .

m2:Step a m2:MetaConcept,
        <owl:NamedIndividual> ;
    ns2:created "2026-01-28" ;
    ns2:creator "Echopraxium with the collaboration of Claude AI" ;
    m2:containerProperty [ m2:containerTypes "m2:Behavior",
                "m2:Workflow" ;
            m2:relationProperty "m2:componentOf" ;
            ns1:comment "Step is component of Behavior or Workflow" ] ;
    m2:discoveryContext "Emerged from analysis of sequential systems (Butterfly lifecycle, Blood pressure control). Needed specialized Node type for temporal contexts that can trigger atomic Actions." ;
    m2:distinctionFromRelated [ ] ;
    m2:eagleView [ ] ;
    m2:hasCategory "m2:Structural" ;
    m2:hasDominantM3 "m3:eagle_eye:Dynamics",
        "m3:eagle_eye:Information",
        "m3:eagle_eye:Structure" ;
    m2:hasEpistemicGap 2e-01 ;
    m2:hasExample "Butterfly egg stage (triggers cell division actions)",
        "HTTP parse step (triggers tokenize, validate actions)",
        "Learning epoch (triggers forward, backward, update actions)",
        "Recipe step 'make roux' (triggers heat, stir, add actions)" ;
    m2:hasPolarity "dual" ;
    m2:hasTensorFormula "S⊗I⊗D" ;
    m2:inheritedFrom "m2:Node" ;
    m2:perspective "dual" ;
    m2:polarityDescription [ ] ;
    m2:specialization [ ] ;
    m2:sphinxView [ ] ;
    m2:triggerProperty [ m2:cardinalityMin 1 ;
            m2:relationProperty "m2:triggers" ;
            m2:targetType "m2:Action" ;
            ns1:comment "Step triggers one or more Actions" ] ;
    ns1:comment "Node specialized for sequential/temporal contexts. Elementary unit in Behavior or Workflow network. Extends Node by adding temporal/sequential semantics and capability to trigger Actions." ;
    ns1:label "Step" ;
    ns1:subClassOf "m2:Node" .

m2:Tropism a m2:MetaConcept,
        <owl:NamedIndividual> ;
    ns2:created "2026-01-28" ;
    ns2:creator "Echopraxium with the collaboration of Claude AI" ;
    m2:discoveryContext "Recognized as specialized Behavior pattern prevalent in biological systems (phototropism, chemotaxis, geotropism). Extends Behavior by adding Attractor dimension to capture directional constraint." ;
    m2:distinctionFromRelated [ ] ;
    m2:eagleView [ ] ;
    m2:gradientProperty [ m2:relationProperty "m2:hasGradient" ;
            m2:sourceType "m3:eagle_eye:Attractor" ;
            ns1:comment "Tropism is directed by an Attractor gradient" ] ;
    m2:hasCategory "m2:Dynamic" ;
    m2:hasDominantM3 "m3:eagle_eye:Attractor",
        "m3:eagle_eye:Dynamics",
        "m3:eagle_eye:Flow",
        "m3:eagle_eye:Structure" ;
    m2:hasEpistemicGap 1.8e-01 ;
    m2:hasExample "Chemotaxis (bacteria towards nutrient gradient)",
        "Geotropism (plant roots towards gravity)",
        "Negative phototropism (roots away from light)",
        "Phototropism (plant stem towards light source)",
        "Thermotaxis (organism towards optimal temperature)" ;
    m2:hasPolarity "dual" ;
    m2:hasTensorFormula "A⊗S⊗D⊗F" ;
    m2:inheritedFrom "m2:Behavior" ;
    m2:perspective "dual" ;
    m2:polarityDescription [ ] ;
    m2:specialization [ ] ;
    m2:sphinxView [ ] ;
    ns1:comment "Specialized Behavior directed by external gradient stimulus. Orientation or movement towards (positive) or away from (negative) an Attractor source. Inherits network decomposability from Behavior while adding directional constraint." ;
    ns1:label "Tropism" ;
    ns1:subClassOf "m2:Behavior" .

m2:Workflow a m2:MetaConcept,
        <owl:NamedIndividual> ;
    ns2:created "2026-01-28" ;
    ns2:creator "Echopraxium with the collaboration of Claude AI" ;
    m2:discoveryContext "Needed to distinguish prescriptive models (Workflow) from descriptive observations (Behavior). Both are structured networks of Steps, but serve different epistemic roles." ;
    m2:distinctionFromRelated [ ] ;
    m2:eagleView [ ] ;
    m2:hasCategory "m2:Structural" ;
    m2:hasDominantM3 "m3:eagle_eye:Dynamics",
        "m3:eagle_eye:Flow",
        "m3:eagle_eye:Structure" ;
    m2:hasEpistemicGap 2.2e-01 ;
    m2:hasExample "Clinical protocol (diagnosis-treatment-followup procedure)",
        "Manufacturing process workflow (assembly steps)",
        "Recipe (cooking steps with dependencies)",
        "Software CI/CD pipeline (build-test-deploy sequence)" ;
    m2:hasPolarity "dual" ;
    m2:hasTensorFormula "S⊗D⊗F" ;
    m2:implementationProperty [ m2:relationProperty "m2:implementsProcess" ;
            m2:semantics "One Process can have multiple Workflow implementations" ;
            m2:targetType "m2:Process" ;
            ns1:comment "Workflow implements Process" ] ;
    m2:perspective "dual" ;
    m2:polarityDescription [ ] ;
    m2:sphinxView [ ] ;
    ns1:comment "Structured network implementation of a Process. Prescriptive representation (Map-oriented) of how a Process should be executed, distinct from Behavior (descriptive observation of what occurs). Can be simulated or prescribed." ;
    ns1:label "Workflow" .

