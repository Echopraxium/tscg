m1core:ModeratorMechanism a owl:Class ;
    rdfs:label "Moderator Mechanism" ;
    rdfs:comment "Component or process that slows, controls, or stabilizes a rapid/intense process. Function: energy reduction, rate control, stabilization. Examples: neutron moderator (nuclear), enzyme inhibitor (biology), damper (mechanics), low-pass filter (electronics)." ;
    skos:definition "Regulatory element that reduces process intensity or rate without stopping it entirely. Acts as intermediate control between full activity and complete shutdown." ;
    skos:related "m1core:SelfSustainingReaction",
        "m2:Amplification",
        "m2:Regulation" ;
    m1core:designPrinciples "Continuous action (not binary on/off)",
        "Energy dissipation or transfer",
        "Negative feedback (stabilizing)",
        "Partial control (reduce, not eliminate)" ;
    m1core:discoveryContext "Nuclear reactor moderator (water, graphite) slowing fast neutrons revealed universal pattern of rate control in rapid processes." ;
    m1core:m2Basis "m2:Amplification (direction: attenuating)",
        "m2:Homeostasis",
        "m2:Regulation" ;
    m1core:primaryFunctions "Bandwidth limiting (filter high frequencies)",
        "Energy reduction (fast → slow)",
        "Rate control (limit reaction speed)",
        "Stabilization (prevent runaway)" ;
    m1core:tensorFormula "Regulation ⊗ Attenuation ⊗ Stabilization" ;
    m1core:transdisciplinaryExamples [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ] ;
    m1core:validationCount 7 .

m1core:PassiveSafety a owl:Class ;
    rdfs:label "Passive Safety" ;
    rdfs:comment "Safety mechanism requiring NO external power or human intervention. Relies on physics (gravity, natural circulation), material properties (melting points), or fail-safe design. Examples: HTR natural cooling, MSR freeze plug, automotive crumple zones, dead man's switch." ;
    skos:definition "Design philosophy where safe state is achieved through inherent physical principles (gravity, thermodynamics, material properties) rather than active systems requiring power or operator action. Fail-safe by default." ;
    skos:related "m1core:ModeratorMechanism",
        "m2:Constraint",
        "m2:Resilience" ;
    m1core:contrastWithActiveSafety [ ] ;
    m1core:designPrinciples "Fail-safe (safe state is default, requires no energy)",
        "Inherent property (not added feature)",
        "No external power needed",
        "No operator action required",
        "Physics-based (gravity, natural circulation, material limits)" ;
    m1core:discoveryContext "HTR and MSR passive safety features revealed universal design pattern of physics-based fail-safe mechanisms." ;
    m1core:m2Basis "m2:Constraint",
        "m2:Resilience",
        "m2:Structure" ;
    m1core:nuclearSafetyExample [ ] ;
    m1core:tensorFormula "Constraint ⊗ Resilience ⊗ InherentProperty" ;
    m1core:transdisciplinaryExamples [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ] ;
    m1core:validationCount 7 .

m1core:SelfSustainingReaction a owl:Class ;
    rdfs:label "Self-Sustaining Reaction" ;
    rdfs:comment "Process where products enable continuation of process (output → input positive feedback loop). Requires critical mass/threshold to initiate. Examples: fission chain reaction, combustion, autocatalysis, viral spread." ;
    skos:definition "Dynamic process characterized by positive feedback where reaction products trigger new reactions, creating potential for exponential growth if unregulated. Always exhibits threshold behavior (critical point)." ;
    skos:related "m1core:CriticalityRegime",
        "m1core:ModeratorMechanism",
        "m2:Amplification",
        "m2:Cycle" ;
    m1core:controlStrategies "Negative feedback (homeostasis)",
        "Product removal (interrupt cycle)",
        "Rate reduction (moderator mechanism)",
        "Threshold maintenance (critical point)" ;
    m1core:discoveryContext "Nuclear fission chain reaction analysis revealed universal pattern of output → input positive feedback with threshold behavior." ;
    m1core:keyCharacteristic "Output → Input (positive feedback creates self-reinforcing loop)" ;
    m1core:m2Basis "m2:Amplification",
        "m2:Cycle",
        "m2:Process",
        "m2:Threshold" ;
    m1core:requiredConditions "Amplification (gain ≥ 1 per cycle)",
        "Initial trigger (seed, spark, catalyst)",
        "Positive feedback mechanism (output → input)",
        "Threshold (critical point for self-sustaining)" ;
    m1core:tensorFormula "Process ⊗ Cycle ⊗ Amplification (gain ≥ 1)" ;
    m1core:transdisciplinaryExamples [ ],
        [ ],
        [ ],
        [ ],
        [ ],
        [ ] ;
    m1core:validationCount 6 .

