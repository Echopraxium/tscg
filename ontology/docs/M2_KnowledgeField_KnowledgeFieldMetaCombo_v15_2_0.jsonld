    {
      "@id": "m2:KnowledgeFieldMetaCombo",
      "@type": [
        "owl:NamedIndividual",
        "m2:MetaConcept"
      ],
      "rdfs:label": "KnowledgeFieldMetaCombo (Disciplinary Specialization)",
      "rdfs:comment": "Factorization metaconcept coupling a KnowledgeField (disciplinary tag, ADJECTIVE form) with one or more universal Metaconcepts to create domain-specific specializations. Prevents M2 pollution by defining domain-specific concept variants only once at M2 level, with concrete instantiation deferred to M1 extensions. Uses qualification operator (⊙) rather than tensor product (⊗) - the result preserves the metaconcept's dimensions while adding disciplinary context. Critical constraint: can ONLY be instantiated in M1 domain extensions, NEVER directly in M2.",
      "m2:hasCategory": "m2:Compositional",
      "m2:hasTensorFormula": "KnowledgeField ⊙ Metaconcept(s)",
      "m2:hasTensorFormulaTeX": "\\text{KnowledgeField} \\odot \\bigotimes_{i=1}^{n} M_i",
      "m2:hasTensorFormulaASCII": "KnowledgeField (o) (x)_i=1^n M_i",
      "m2:hasDominantM3": [
        "m3:eagle_eye:Structure",
        "m3:eagle_eye:Information"
      ],
      "m2:hasEpistemicGap": 0.3,
      "m2:hasPolarity": "neutral",
      "m2:perspective": "dual",
      
      "m2:combinationFormula": {
        "description": "Disciplinary qualification of universal metaconcept pattern",
        "formula": "KnowledgeField ⊙ Metaconcept(s)",
        "components": [
          {
            "type": "KnowledgeField",
            "form": "ADJECTIVE instance (Chemical, Biological, Mechanical...)",
            "role": "Disciplinary qualifier - provides domain context",
            "arity": 1,
            "source": "Derived from NOUN KnowledgeField instance (Chemistry → Chemical)"
          },
          {
            "type": "Metaconcept",
            "role": "Universal pattern to specialize",
            "arity": "1+",
            "examples": ["Link", "Processor", "Component", "Trigger", "Transformation"]
          }
        ]
      },
      
      "m2:operatorSemantics": {
        "symbol": "⊙",
        "name": "Disciplinary Qualification Operator",
        "type": "Contextual projection (not tensor fusion)",
        "formalDefinition": "(KnowledgeField ⊙ Metaconcept)(x) = Metaconcept(x | context=KnowledgeField)",
        "dimensionality": "Preserves Metaconcept dimensions - NOT emergent",
        "difference_from_tensor": {
          "tensor_product": "⊗ creates emergent 25D space from two 5D metaconcepts",
          "qualification": "⊙ preserves original 5D space with added context tag"
        },
        "interpretation": "Result is the SAME metaconcept pattern, specialized to disciplinary context",
        "example": "Chemical ⊙ Link = Link pattern (S⊗F) operating within chemistry context (bonds, reactions, pathways)"
      },
      
      "m2:instantiationConstraint": {
        "level": "M1 extensions ONLY",
        "rationale": "KnowledgeFieldMetaCombo is abstract factorization pattern defined in M2. Concrete instances (ChemicalLink, BiologicalProcessor, MechanicalComponent) can ONLY be created within appropriate M1 domain extensions to prevent M2 pollution with domain-specific variants.",
        "enforcement": "M2 contains ONLY the abstract KnowledgeFieldMetaCombo pattern; M1_Chemistry, M1_Biology, M1_Engineering... contain concrete Concept instances",
        "violation_example": "Creating ChemicalLink directly in M2 would pollute the universal metaconcept layer with chemistry-specific details",
        "correct_pattern": "M2 defines KnowledgeFieldMetaCombo pattern → M1_Chemistry instantiates ChemicalLink using pattern"
      },
      
