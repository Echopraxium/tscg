  "@graph": [
    {
      "@id": "m2:KnowledgeField",
      "@type": [
        "owl:NamedIndividual",
        "m2:MetaConcept"
      ],
      "rdfs:label": "KnowledgeField (Hybrid Tensor Product ASFID⊗REVOI)",
      "rdfs:comment": "Structured field of knowledge where Territory phenomena (ASFID) and Map frameworks (REVOI) are coupled through epistemic practices. Dual role: (1) NOUN instances (Chemistry, Biology, Optics...) represent complete epistemic fields with full ASFID⊗REVOI theory - defined in M1 extensions; (2) ADJECTIVE instances (Chemical, Biological, Optical...) serve as disciplinary tags for domain-specific metaconcept specialization - defined in M1 extensions. Reduced to 5D via SVD from full 25D ASFID⊗REVOI tensor product space.",
      "m2:hasCategory": "m2:Ontological",
      "m2:hasTensorFormula": "∑ᵢ σᵢ |uᵢ⟩⊗|vᵢ⟩ (5D SVD, ASFID ⊗ REVOI)",
      "m2:hasTensorFormulaTeX": "\\sum_{i=1}^{5} \\sigma_i |u_i\\rangle \\otimes |v_i\\rangle \\quad (\\text{5D SVD}, \\; ASFID \\otimes REVOI)",
      "m2:hasTensorFormulaASCII": "sum_i sigma_i |u_i> (x) |v_i> (5D SVD, ASFID (x) REVOI)",
      "m2:hasDominantM3": [
        "HYBRID: Both ASFID (Eagle Eye) and REVOI (Sphinx Eye) simultaneously",
        "Territory-Map coupling encoded in matrix α"
      ],
      "m2:hasEpistemicGap": 0.4,
      "m2:hasPolarity": "hybrid",
      "m2:perspective": "hybrid",
      
      "m2:dualRole": {
        "description": "KnowledgeField has two distinct instantiation modes, both crucial to TSCG architecture",
        "asEpistemicField": {
          "description": "NOUN instances representing complete knowledge fields",
          "instances": ["Chemistry", "Biology", "Optics", "Mechanics", "Physics", "Electronics"],
          "location": "M1 extensions (M1_Chemistry.jsonld, M1_Biology.jsonld, M1_Optics.jsonld...)",
          "theory": "Full ASFID⊗REVOI coupling, SVD decomposition, epistemic depth, domain maturity metrics",
          "tensorFormula": "∑ᵢ σᵢ |uᵢ⟩⊗|vᵢ⟩",
          "stateVector": "α_T |Territory⟩ + α_M |Map⟩ + ∑ᵢ σᵢ|ASFID_i⊗REVOI_i⟩",
          "purpose": "Represent complete scientific/professional domains with their epistemic structures",
          "example": "Chemistry KnowledgeField (NOUN) includes all chemical theory, observable chemical phenomena, coupling between molecular models and experimental chemistry"
        },
        "asDisciplinaryTag": {
          "description": "ADJECTIVE instances serving as disciplinary qualifiers",
          "instances": ["Chemical", "Biological", "Optical", "Mechanical", "Electronic", "Thermal"],
          "location": "Within M1 extensions as qualifiers for domain-specific concepts",
          "theory": "Lightweight projection - derives adjectival form from parent epistemic field without inheriting full theory",
          "relation": "Chemical (adjective) derives from Chemistry (noun) KnowledgeField",
          "purpose": "Tag universal metaconcepts with domain context to create specialized concepts",
          "usage": "Exclusively used in KnowledgeFieldMetaCombo pattern",
          "example": "Chemical (adjective) tags Link metaconcept → ChemicalLink concept in M1_Chemistry"
        }
      },
      
      "m2:hybridView": {
        "description": "Unique bicephalous fusion - first metaconcept requiring BOTH perspectives simultaneously",
        "territoryComponent": {
          "basis": "ASFID",
          "role": "Observable phenomena in field (Eagle Eye)",
          "weight": "α_T > 0.5 (mandatory bicephality)"
        },
        "mapComponent": {
          "basis": "REVOI",
          "role": "Conceptual frameworks in field (Sphinx Eye)",
          "weight": "α_M > 0.5 (mandatory bicephality)"
        },
        "couplingMatrix": {
          "symbol": "α",
          "dimension": "5×5",
          "role": "Encodes ASFID-REVOI interaction strength",
          "sparsity": "~80% (5 non-zero terms from 25 possible)",
          "computation": "Empirical from field analysis"
        },
        "svdDecomposition": {
          "fullSpace": "ℂ²⁵ (25-dimensional ASFID⊗REVOI tensor product)",
          "reducedSpace": "ℂ⁵ (5 principal modes via SVD)",
          "singularValues": "[σ₁, σ₂, σ₃, σ₄, σ₅] with σ₁ ≥ σ₂ ≥ ... ≥ σ₅",
          "dominantModes": "First 2-3 modes capture 80%+ variance"
        }
      },
      
      "m2:mathematicalFormulation": {
        "fullTensorProduct": "KnowledgeField = ℋ_ASFID ⊗ ℋ_REVOI = ℂ²⁵",
        "reductionPrinciple": "dim(KnowledgeField) ≈ 5 via SVD (parsimony)",
        "stateVector": "|KnowledgeField⟩ = α_T |Territory⟩ + α_M |Map⟩ + ∑ᵢ σᵢ|ASFID_i⊗REVOI_i⟩",
        "bicephalityConstraint": "α_T > 0.5 AND α_M > 0.5 (both perspectives mandatory)",
        "epistemicDepthMetric": "Epistemic_Depth(KF) = ||ASFID|| · ||REVOI|| · cos(θ)",
        "alignmentAngle": {
          "θ≈0°": "Mature field (e.g., Newtonian mechanics)",
          "θ≈45°": "Developing field (e.g., early quantum mechanics)",
          "θ≈90°": "Crisis or pseudoscience (Territory-Map disconnect)"
        }
      },
      
      "m2:5DDecomposition": {
        "description": "Each knowledge field exhibits 5 principal dimensions from ASFID-REVOI coupling",
        "term1": "Dominant ASFID dimension (Territory anchor)",
        "term2": "Dominant REVOI dimension (Map anchor)",
        "term3": "Primary ASFID⊗REVOI coupling (epistemic mode 1)",
        "term4": "Secondary ASFID⊗REVOI coupling (epistemic mode 2)",
        "term5": "Tertiary ASFID⊗REVOI coupling (epistemic mode 3)",
        "parsimonyRationale": "5D balances: (a) Rigor (mathematical tractability), (b) Richness (captures field complexity), (c) Interpretability (human-understandable)"
      },
      
