      "m2:relationToMetaconceptCombo": {
        "similarity": "Both are compositional patterns combining simpler elements",
        "criticalDifference": {
          "MetaconceptCombo": {
            "formula": "M₁ ⊗ M₂ ⇒ M_emergent",
            "operation": "Tensor fusion",
            "result": "Emergent metaconcept with NEW properties (25D from two 5D)",
