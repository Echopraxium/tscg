<https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M3_GenesisSpace.jsonld#category_theory:morphismSource> a owl:ObjectProperty ;
    rdfs:label "morphism source" ;
    rdfs:comment "The source (domain) object of a morphism." ;
    rdfs:domain "m3:category_theory:Morphism" ;
    rdfs:range "m3:category_theory:CategoryObject" .

<https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M3_GenesisSpace.jsonld#category_theory:morphismTarget> a owl:ObjectProperty ;
    rdfs:label "morphism target" ;
    rdfs:comment "The target (codomain) object of a morphism." ;
    rdfs:domain "m3:category_theory:Morphism" ;
    rdfs:range "m3:category_theory:CategoryObject" .

<https://raw.githubusercontent.com/Echopraxium/tscg/main/ontology/M3_GenesisSpace.jsonld#category_theory:morphismType> a owl:DatatypeProperty ;
    rdfs:label "morphism type" ;
    rdfs:comment "Type classification of morphism (identity, isomorphism, inclusion, etc.)." ;
