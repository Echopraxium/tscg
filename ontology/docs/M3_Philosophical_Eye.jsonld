    "dcterms": "http://purl.org/dc/terms/"
  },
  "@graph": [
    {
      "@id": "m3:PhilosophicalEye",
      "@type": "owl:Ontology",
      "dcterms:title": "TSCG M3 - Philosophical Eye (Lion Eye)",
      "dcterms:description": "Philosophical / Qualitative perspective of the bicephalous architecture. The Lion eye provides interpretation, meaning-making, contextualization and subjective understanding. Focuses on WHY and WHAT FOR systems exist (purpose, significance, value).",
      "dcterms:creator": "TSCG Project - Michel Favre",
      "dcterms:created": "2026-01-17",
      "dcterms:modified": "2026-01-17",
      "owl:versionInfo": "1.0.0",
      "m3:metaphor": "Lion - King of beasts, symbol of wisdom and majesty. Represents depth, meaning, understanding of essence.",
      "m3:role": "Interpretation, Meaning-Making, Contextualization, Purpose",
      "m3:complementaryTo": {
        "@id": "m3:MathematicalEye",
        "rdfs:seeAlso": "https://github.com/Echopraxium/tscg/blob/main/ontology/M3_Mathematical_Eye.jsonld",
        "dcterms:documentation": {
          "@id": "https://github.com/Echopraxium/tscg/blob/main/ontology/M3_Mathematical_Eye.jsonld",
          "dcterms:format": "application/ld+json",
          "dcterms:type": "Ontology"
        }
      },
      "m3:characteristics": {
        "precision": "Moderate - focuses on qualitative understanding rather than exact values",
        "abstraction": "High - uses concepts, metaphors, philosophical frameworks",
        "subjectivity": "Acknowledged - recognizes observer-dependence and perspectivism",
        "scope": "Synthetic - integrates components into meaningful wholes",
        "output": "Interpretations, narratives, meanings, purposes, values"
      },
      "m3:capabilities": [
        {
          "capability": "Interpretation",
          "description": "Extract meaning and significance from measurements",
          "examples": [
            "ΔΘ=0.35 → 'RGB is a good model but simplifies continuous spectrum'",
            "F=0.90 in Fire → 'High flow indicates energy-intensive process'",
            "K channel in CMYK → 'Practical optimization driven by economic constraints'",
            "HSL cylinder → 'Perceptual organization matches human color intuition'"
          ]
        },
        {
          "capability": "Contextualization",
          "description": "Situate systems within broader frameworks",
          "examples": [
            "Fire Triangle in Chemistry: Safety tool for firefighters",
            "RGB in Computing: Display technology standard since 1990s",
            "CMYK in Printing: Industrial evolution from CMY (imperfect pigments)",
            "Map-Territory: Korzybski's General Semantics (1933)"
          ]
        },
        {
          "capability": "Purpose Identification",
          "description": "Understand WHY and WHAT FOR systems exist",
          "examples": [
            "Fire Triangle: Pedagogical simplification for fire prevention",
            "RGB: Matches human trichromatic vision (biological constraint)",
            "HSL: Intuitive color selection for designers (usability goal)",
            "CMYK: Cost-effective high-quality printing (practical need)"
          ]
        },
        {
          "capability": "Limitation Analysis",
          "description": "Identify what models cannot capture",
          "examples": [
            "RGB gamut < Visible spectrum (not all colors representable)",
            "CMY(100,100,100) ≠ Black (pigment impurity)",
            "HSL not perceptually uniform (ΔE ≠ ΔHSL)",
            "Fire Triangle excludes chain reaction (simplified model)"
          ]
        },
        {
          "capability": "Value Assessment",
          "description": "Evaluate significance and worth",
          "examples": [
            "RGB: High value for screens, zero value for print",
            "CMYK: Essential for printing industry, irrelevant for displays",
            "Epistemic gap ΔΘ: Small = good model, large = poor model",
            "Metaconcept coverage: 15/52 (29%) = reasonable for poclet"
          ]
        }
      ],
      "m3:methodologies": [
        {
          "methodology": "Hermeneutic Circle",
          "description": "Iterative interpretation of parts and whole",
          "procedure": [
            "1. Understand individual components (R, G, B channels)",
            "2. Grasp whole system (RGB color space)",
            "3. Reinterpret components in light of whole",
            "4. Refine understanding iteratively"
          ],
          "output": "Holistic understanding"
        },
        {
          "methodology": "Phenomenological Bracketing",
          "description": "Suspend assumptions to examine phenomena directly",
          "procedure": [
            "1. Identify phenomenon (e.g., perceived color)",
            "2. Bracket preconceptions (forget formulas, models)",
            "3. Describe direct experience (qualia, perception)",
            "4. Relate back to models (Territory vs Map)"
          ],
          "output": "Experiential description"
        },
        {
          "methodology": "Dialectical Analysis",
          "description": "Explore tensions and contradictions",
          "procedure": [
            "1. Identify thesis (RGB additive model)",
            "2. Identify antithesis (CMY subtractive model)",
            "3. Seek synthesis (Both valid for different media)",
            "4. Recognize higher-order unity (Color as phenomenon)"
          ],
          "output": "Synthetic understanding"
        },
        {
          "methodology": "Historical Genealogy",
          "description": "Trace conceptual evolution over time",
          "procedure": [
            "1. Identify current concept (CMYK printing)",
            "2. Trace historical development (CMY → CMYK in 20th century)",
            "3. Understand driving forces (economic, technical)",
            "4. Appreciate contingency (could have evolved differently)"
          ],
          "output": "Historical narrative"
        }
      ],
      "m3:tools": [
        "Conceptual analysis (definitions, distinctions, relations)",
        "Metaphor and analogy (Map-Territory, Eagle-Lion)",
        "Narrative and storytelling (historical accounts, case studies)",
        "Critical questioning (Why? What for? What if?)",
        "Perspectivism (multiple viewpoints, observer-dependence)",
        "Ethical reflection (values, goods, purposes)",
        "Aesthetic judgment (beauty, elegance, coherence)"
      ],
      "m3:strengths": [
        "Meaning and significance (answers WHY questions)",
        "Contextual sensitivity (adapts to domain and purpose)",
        "Holistic understanding (sees forest, not just trees)",
        "Human relevance (connects to values, experiences)",
        "Flexibility (accommodates ambiguity, paradox)"
      ],
      "m3:limitations": [
        "Subjectivity (interpretations may vary)",
        "Non-reproducibility (context-dependent insights)",
        "Verbosity (harder to communicate precisely)",
        "Non-computable (machines cannot interpret meaning)",
        "Slower (requires reflection, not just calculation)"
      ],
      "m3:examplesInTSCG": [
        {
          "context": "Fire Triangle Territory",
          "lionView": "Combustion is emergent process requiring three-way synergy. Removal of any component breaks the system → fire stops. This mirrors biological systems (organism dies without food, water, or oxygen) and social systems (society needs resources, organization, energy).",
          "interpretation": "Fire Triangle is transdisciplinary pattern (synergistic three-component systems)",
          "significance": "Teaching fire safety, understanding emergence, designing suppression strategies"
        },
        {
          "context": "RGB vs CMYK",
          "lionView": "Same phenomenon (color perception), two Maps optimized for different media. RGB for light emission (screens), CMYK for light absorption (print). Neither is 'more correct' - appropriateness depends on application context.",
          "interpretation": "Validates Korzybski: 'Map ≠ Territory' - multiple valid representations",
          "significance": "Designers must choose right model for medium (RGB for web, CMYK for print)"
        },
        {
          "context": "Epistemic Gap ΔΘ",
          "lionView": "Gap quantifies Map-Territory divergence, but interpretation depends on purpose. ΔΘ=0.35 for RGB: 'Good model for displays, acceptable simplification of continuous spectrum.' But for spectroscopy, ΔΘ=0.35 would be unacceptable (need full spectral resolution).",
          "interpretation": "Gap tolerance is context-dependent (purpose-relative)",
          "significance": "No universal threshold - 'good enough' depends on application"
        },
        {
          "context": "Channel Metaconcept",
          "lionView": "Identified through poclet modeling (RGB revealed need for 'conduit' concept). Channel bridges Component (part) and Signal (content) - it's the structured pathway. Transdisciplinary: RGB channels, audio channels, ion channels, distribution channels.",
          "interpretation": "Metaconcepts emerge from practice (bottom-up discovery)",
          "significance": "Framework evolves through use (M2 grows as we model more domains)"
        }
      ],
      "m3:bicephalousIntegration": {
        "description": "Philosophical Eye provides MEANING; Mathematical Eye provides MEASUREMENT",
        "synergy": "Together, they create understanding (knowledge + significance)",
        "workflow": [
          "1. Lion asks: What are we modeling? Why? What for?",
          "2. Eagle measures: ASFID coefficients, formulas, gaps",
          "3. Lion interprets: What do these numbers mean? Limitations?",
          "4. Binocular synthesis: Quantified, contextualized understanding"
        ],
        "analogy": "Eagle sees HOW MUCH (measurements); Lion sees WHY and WHAT FOR (meaning, purpose, value)"
      },
      "m3:philosophicalTraditions": [
        {
          "tradition": "Phenomenology (Husserl, Heidegger)",
          "relevance": "Emphasis on lived experience, bracketing, phenomena as they appear"
        },
        {
          "tradition": "Hermeneutics (Gadamer, Ricoeur)",
          "relevance": "Interpretation, hermeneutic circle, understanding through dialogue"
        },
        {
          "tradition": "Pragmatism (Peirce, James, Dewey)",
          "relevance": "Truth as what works, meaning in consequences, inquiry as problem-solving"
        },
        {
          "tradition": "Constructivism (von Glasersfeld, Piaget)",
          "relevance": "Knowledge actively constructed, observer-dependence, viability over truth"
        },
        {
          "tradition": "General Semantics (Korzybski)",
          "relevance": "Map ≠ Territory, levels of abstraction, non-Aristotelian logic"
        },
        {
          "tradition": "Cybernetics (Wiener, Bateson, von Foerster)",
          "relevance": "Circular causality, observer as part of system, second-order cybernetics"
        }
      ],
      "m3:questionsLionAsks": [
        "What is the purpose of this system/model?",
        "Why was it designed this way?",
        "What does it enable or constrain?",
        "What are the implicit assumptions?",
        "Who benefits from this representation?",
        "What alternatives were rejected?",
        "What is left out or excluded?",
        "How does context shape interpretation?",
        "What values are embedded in the design?",
        "What is the historical/cultural background?"
      ],
      "m3:relatedConcepts": [
        {
          "concept": "Verstehen (Weber)",
          "relation": "Understanding from within, empathetic interpretation"
        },
        {
          "concept": "Bildung (German philosophy)",
          "relation": "Cultivation, formation, holistic education"
        },
        {
          "concept": "Phronesis (Aristotle)",
          "relation": "Practical wisdom, knowing what to do in context"
        },
        {
          "concept": "Telos (Aristotle)",
          "relation": "Purpose, final cause, what something is for"
        }
      ],
