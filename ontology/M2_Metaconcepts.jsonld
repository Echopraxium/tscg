          "m2:description": "Structural pattern of network connectivity (graph topology)",
          "m2:possibleValues": [
            {
              "value": "Scale-free",
              "alternative_names": [
                "Power-law",
                "Preferential attachment"
              ],
              "description": "Degree distribution follows power law P(k) ~ k^(-γ), hubs exist",
              "formula": "P(k) = Ck^(-γ), typically γ ∈ [2, 3]",
              "examples": [
                "Internet (router connectivity)",
                "World Wide Web (hyperlinks)",
                "Protein interaction networks",
                "Social networks (Twitter, Facebook follows)",
                "Citation networks (papers citing papers)",
                "Metabolic networks"
              ],
              "characteristics": "Few highly connected hubs, many low-degree nodes, no characteristic scale",
              "formation": "Preferential attachment (Barabási-Albert model): 'rich get richer'",
              "robustness": "Robust to random failures, vulnerable to targeted hub attacks",
              "clustering": "Moderate clustering coefficient",
              "diameter": "Small (logarithmic scaling)",
              "degree_distribution": "Heavy-tailed (power law, not Poisson)"
            },
            {
              "value": "Small-world",
              "alternative_names": [
                "Watts-Strogatz"
              ],
              "description": "High clustering + short path length (six degrees of separation)",
              "formula": "L ~ log(N) (small characteristic path length), C >> C_random (high clustering)",
              "examples": [
                "Social networks (friends-of-friends)",
                "Neural networks (C. elegans connectome)",
                "Power grid",
                "Collaboration networks (Erdős numbers)",
                "Airline routes"
              ],
              "characteristics": "Combines local clustering (cliques) with long-range shortcuts",
              "formation": "Watts-Strogatz: start with ring lattice, rewire with probability p",
              "robustness": "Moderately robust",
              "clustering": "High (like regular lattice)",
              "diameter": "Small (like random graph)",
              "phenomenon": "Six degrees of separation (Kevin Bacon game)"
            },
            {
              "value": "Random",
              "alternative_names": [
                "Erdős-Rényi",
                "Poisson"
              ],
              "description": "Edges placed randomly with probability p",
              "formula": "P(k) ~ Poisson(μ), μ = p(N-1) (degree distribution)",
              "examples": [
                "Random chemical reactions (early universe)",
                "Random graphs (mathematical model)",
                "Baseline/null model for network comparison"
              ],
              "characteristics": "Homogeneous, no structure, theoretical baseline",
              "formation": "Erdős-Rényi: each edge exists independently with probability p",
              "robustness": "Moderate (homogeneous)",
              "clustering": "Low (C ~ p)",
              "diameter": "Small (logarithmic)",
              "degree_distribution": "Poisson (narrow, bell-shaped)",
              "phase_transition": "Giant component emerges at p_c = 1/N"
