  "foundation_layer": {
    "@id": "M3_GenesisSpace",
    "@type": "owl:imports",
    "relationship": "derived_from",
    "description": "M2 tensor space is derived from M3 Genesis Space through dual mechanisms: Eagle Eye projects ASFID dimensions analytically, Sphinx Eye constructs patterns synthetically",
    
    "derivation_mechanisms": [
      {
        "source": {
          "@id": "M3_EagleEye",
          "basis": "ASFID"
        },
        "operation": "projection",
        "operator": "Π_Eagle: M3_Eagle → M2",
        "description": "ASFID basis projects 5 dimensions onto each M2 metaconcept, providing analytical perspective",
        "effect": "Each metaconcept receives ASFID decomposition: (A, S, F, I, D) coordinates"
      },
      {
        "source": {
          "@id": "https://tscg.org/M3_SphinxEye.jsonld",
          "basis": "ORIVE"
        },
        "operation": "construction",
        "operator": "Σ_Sphinx: ORIVE × M2 → M2_patterns",
        "description": "ORIVE basis constructs M2 patterns through synthetic composition",
        "effect": "Metaconcepts compose into domain patterns via ORIVE operations"
      }
    ],
    
    "mathematical_relationship": "M2 ⊂ span(M3_GenesisSpace)",
    "interpretation": "M2 tensor space is contained within the space spanned by M3 Genesis Space basis"
  },
  
  "tensor_structure": {
    "description": "M2 metaconcepts form tensor products of M3 Genesis Space basis elements",
    "formalism": "M2_metaconcept ≅ ASFID_projection ⊗ ORIVE_construction",
    "dimensionality": "Each metaconcept lives in 10D space (5D ASFID × 5D ORIVE)",
    "representation": "Metaconcepts are tensors with ASFID and ORIVE indices"
  },
  
