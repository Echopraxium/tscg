  "metaconcepts": [
    {
      "id": "mc_001",
      "name": "Imbrication",
      "category": "Structural Patterns",
      "date_added": "2026-01-21",
      "status": "validated",
      
      "definition": {
        "short": "Nested systems at different scales with hierarchical encapsulation",
        "long": "Imbrication describes how systems nest within one another at different scales, creating hierarchical encapsulation where each level maintains its own integrity while being contained within larger structures. This is distinct from simple hierarchy (which is about ordering) and composition (which is about assembly)."
      },
      
      "asfid_projections": {
        "attractor": {
          "description": "Nested basins of attraction",
          "example": "Cellular homeostasis within organ-level regulation within organism-level equilibrium"
        },
        "structure": {
          "description": "Structural nesting creating matryoshka-like architectures",
          "example": "Atomic → molecular → cellular → tissue → organ → organism hierarchy"
        },
        "flow": {
          "description": "Multi-scale flows where micro-flows are embedded within macro-flows",
          "example": "Turbulent eddies within eddies, blood flow in capillaries within vessels within circulatory system"
        },
        "information": {
          "description": "Nested encodings where information is layered at different abstraction levels",
          "example": "Bits → bytes → data structures → databases → knowledge systems"
        },
        "dynamics": {
          "description": "Coupled dynamics across nested scales where fast dynamics at small scales interact with slow dynamics at large scales",
          "example": "Molecular vibrations → chemical reactions → metabolic pathways → physiological rhythms"
        }
      },
      
      "domain_instantiations": [
        {
          "domain": "Biology",
          "example": "Organelle → cell → tissue → organ → organism → population → ecosystem → biosphere",
          "notes": "Classic example of biological organization"
        },
        {
          "domain": "Computing",
          "example": "Instruction → function → module → library → application → system → network",
          "notes": "Software architecture imbrication"
        },
        {
          "domain": "Optics",
          "example": "Lens element → lens group → complete optical assembly",
          "notes": "Optical system nesting"
        },
        {
          "domain": "Fire",
          "example": "Molecular oxidation → flame front → fire zone → conflagration",
          "notes": "Nested combustion scales"
        },
        {
          "domain": "Photography",
          "example": "Photon capture → sensor integration → exposure duration → sequence timing",
          "notes": "Temporal scale nesting"
        }
      ],
      
      "relationships_to_other_metaconcepts": [
        {
          "metaconcept": "Hierarchy",
          "relationship": "distinct_from",
          "note": "Imbrication is spatial/scalar nesting; hierarchy is relational ordering"
        },
        {
          "metaconcept": "Recursivity",
          "relationship": "complements",
          "note": "Recursivity is self-similar repetition; imbrication is scale-nested containment"
        },
        {
          "metaconcept": "Emergence",
          "relationship": "enables",
          "note": "Nested levels create conditions for emergent properties at higher scales"
        },
        {
          "metaconcept": "Modularity",
          "relationship": "interacts",
          "note": "Modules can be imbricated to create complex nested architectures"
        }
      ],
      
      "mathematical_formalization": {
        "set_theory": "S₁ ⊂ S₂ ⊂ S₃ ⊂ ... ⊂ Sₙ where each level maintains closure",
        "category_theory": "Functors between nested categories preserving structure",
        "topology": "Nested open sets in topological spaces"
      },
      
      "validation": {
        "universality": "Applies across physical, biological, social, abstract domains",
        "distinctiveness": "Clearly distinguished from related concepts",
        "utility": "Essential for multi-scale system analysis",
        "poclet_coverage": "Validated across Fire Triangle, Exposure Triangle, biological systems"
      }
    }
  ],
  
