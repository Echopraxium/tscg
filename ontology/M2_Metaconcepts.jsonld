              "description": "Regular spatial arrangement (1D ring, 2D grid, 3D cubic, etc.)",
              "examples": [
                "Crystal lattice (atoms in solid)",
                "2D image (pixels in grid)",
                "Cellular automaton (Conway's Life)",
                "Finite element mesh"
              ],
              "characteristics": "Regular, local connectivity, spatial embedding",
              "formation": "Regular tiling of space",
              "robustness": "Vulnerable (no redundancy)",
              "clustering": "High (local neighborhoods)",
              "diameter": "Large (scales with √N for 2D, ∛N for 3D)",
              "degree": "Constant (k = 2d for d-dimensional lattice)",
              "spatial": "Embedded in Euclidean space"
            },
            {
              "value": "Hierarchical",
              "alternative_names": [
                "Tree",
                "Nested",
                "Modular"
              ],
              "description": "Nested modules at multiple scales (self-similar hierarchy)",
              "examples": [
                "Biological taxonomy (kingdom → phylum → class → ...)",
                "Military/corporate hierarchy",
                "File system (directory tree)",
                "Bronchial tree (lungs)",
                "River networks (tributaries)",
                "Vascular tree (arteries/veins)"
              ],
              "characteristics": "Tree-like or nested modules, self-similar across scales",
              "formation": "Recursive subdivision or growth",
              "robustness": "Vulnerable at high-level nodes (single point of failure)",
              "clustering": "Modular (high within modules, low between)",
              "diameter": "Logarithmic (tree) or moderate (modular)",
              "degree_distribution": "Power law possible (scale-free hierarchical)",
              "modularity": "High Q (Newman modularity)"
            },
            {
              "value": "Star",
              "description": "Central hub connected to all nodes, no edges between periphery",
              "examples": [
                "Client-server architecture",
                "Hub airport (spoke-and-hub)",
                "Broadcast network (radio tower → receivers)"
              ],
              "characteristics": "Extreme centralization, single hub",
              "robustness": "Extremely vulnerable (hub is single point of failure)",
              "clustering": "Zero (no triangles)",
              "diameter": 2,
              "degree": "Hub has degree N-1, others have degree 1"
