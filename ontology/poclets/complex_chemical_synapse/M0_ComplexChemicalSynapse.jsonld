            "cofactors": "Vitamins (B6, B12), metal ions (Fe, Cu)"
          },
          "m0:examples": [
            {
              "NT": "Acetylcholine (ACh)",
              "enzyme": "Choline acetyltransferase (ChAT)",
              "reaction": "Acetyl-CoA + Choline → ACh + CoA"
            },
            {
              "NT": "Dopamine",
              "pathway": "Tyrosine → L-DOPA (tyrosine hydroxylase) → Dopamine (DOPA decarboxylase)"
            },
            {
              "NT": "Serotonin (5-HT)",
              "pathway": "Tryptophan → 5-HTP (tryptophan hydroxylase) → 5-HT (aromatic amino acid decarboxylase)"
            },
            {
              "NT": "GABA",
              "enzyme": "Glutamic acid decarboxylase (GAD)",
              "reaction": "Glutamate → GABA + CO₂"
            },
            {
              "NT": "Glutamate",
              "source": "Krebs cycle intermediates, glucose metabolism"
            }
          ],
          "m0:regulation": {
            "feedback": "End-product inhibition (NT levels regulate synthesis)",
            "transcriptional": "Gene expression of synthetic enzymes",
            "substrate": "Availability of precursors (e.g., dietary tryptophan for serotonin)"
          },
          "m0:tensorFormula": "D⊗F⊗I (Dynamics × Flow × Information)",
          "m0:asfidContribution": {
            "D": "Dynamic enzymatic reactions (timescale: seconds to minutes)",
            "F": "Metabolic flow (precursors → NT)",
            "I": "Genetic/metabolic information controlling synthesis rates"
