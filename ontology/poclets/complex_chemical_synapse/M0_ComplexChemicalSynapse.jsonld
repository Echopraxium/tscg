          "m0:interdependence": {
            "fromPole2": "No vesicles → no release",
            "towardsPole4": "No release → no postsynaptic signal",
            "feedback": "Autoreceptors (Pole 4 variant) modulate Pole 3 via Ca²⁺ channels"
          }
