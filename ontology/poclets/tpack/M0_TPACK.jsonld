          "rdfs:label": "Content Knowledge (CK)",
          "m0:metaconceptInstantiation": "m2:Component",
          "m0:shortName": "CK",
          "m0:role": "Subject matter expertise - deep understanding of the discipline being taught",
          "m0:function": "Knowing WHAT to teach - facts, concepts, theories, procedures of the domain",
          "m0:scope": "Domain-specific (mathematics CK ≠ history CK ≠ biology CK)",
