          "rdfs:label": "Technology Knowledge (TK)",
          "m0:metaconceptInstantiation": "m2:Component",
          "m0:shortName": "TK",
          "m0:role": "Knowledge about technology tools, digital resources, systems",
          "m0:function": "Understanding how technologies work, troubleshooting, learning new tools",
          "m0:scope": "Ranges from basic (word processing) to advanced (AI, VR, coding)",
          "m0:examples": [
            "Learning Management Systems (Moodle, Canvas, Blackboard)",
            "Presentation software (PowerPoint, Google Slides, Prezi)",
            "Digital assessment tools (Kahoot, Quizlet, Google Forms)",
            "Video conferencing (Zoom, Teams, Google Meet)",
            "Educational apps (Khan Academy, Duolingo, GeoGebra)"
          ],
          "m0:evolution": {
            "1990s": "Desktop software (Office suite, educational CD-ROMs)",
            "2000s": "Web 2.0 tools (blogs, wikis, YouTube)",
            "2010s": "Mobile apps, tablets (iPads in education)",
            "2020s": "AI tutors, VR/AR, learning analytics, generative AI (ChatGPT, Claude)"
          },
          "m0:challenge": "Rapid obsolescence - TK must be continuously updated",
          "m0:tensorFormula": "S⊗I⊗D",
          "m0:asfidContribution": {
            "S": "Technology infrastructure and architecture",
            "I": "Information about tools and platforms",
            "D": "Rapid change (high dynamics) - technology evolves fast"
          }
        },
        {
          "@id": "m0:Pedagogical_Knowledge",
