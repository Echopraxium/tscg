      "m0:intersections": {
        "description": "TPACK's power lies in intersections - synergistic combinations create emergent knowledge types",
        "total": 7,
        "breakdown": {
          "base_components": 3,
          "pairwise_intersections": 3,
          "triple_intersection": 1
        },
        "pairwiseIntersections": [
          {
            "@id": "m0:PCK",
            "name": "Pedagogical Content Knowledge (PCK)",
            "formula": "PK ∩ CK",
            "origin": "Shulman (1986) - foundation of TPACK",
            "definition": "Knowledge of how to teach specific content - blending pedagogy with subject expertise",
            "examples": [
              "Mathematics: Using manipulatives to teach fractions (pedagogy FOR math content)",
              "Science: Designing experiments to reveal misconceptions about density",
              "History: Using primary source analysis to develop critical thinking (historical method AS pedagogy)",
              "Language: Teaching grammar through authentic writing contexts (not isolated drills)"
            ],
            "emergentProperty": "Transforms generic teaching into discipline-specific instruction",
            "metaconcept": "m2:Synergy",
            "synergyFormula": "PCK = P ⊗ C where PCK ≠ P + C (non-additive)"
          },
          {
            "@id": "m0:TCK",
            "name": "Technological Content Knowledge (TCK)",
            "formula": "TK ∩ CK",
            "definition": "Knowledge of how technology represents and transforms subject matter",
            "examples": [
              "Mathematics: GeoGebra for dynamic geometry (technology changes HOW math is explored)",
              "Science: PhET simulations for physics concepts (virtual labs impossible physically)",
              "Music: Digital Audio Workstations change composition process (multi-track recording)",
              "Geography: GIS (Geographic Information Systems) enable spatial analysis at scale",
              "Literature: Corpus linguistics tools reveal patterns across thousands of texts"
            ],
            "emergentProperty": "Technology isn't neutral delivery - it reshapes the content itself",
            "affordances": "Technology affords new representations, explorations, impossibilities become possible",
            "metaconcept": "m2:Transformation",
            "transformationNote": "Technology TRANSFORMS content (doesn't just deliver it)"
          },
          {
            "@id": "m0:TPK",
            "name": "Technological Pedagogical Knowledge (TPK)",
            "formula": "TK ∩ PK",
            "definition": "Knowledge of how technology can support or constrain pedagogical approaches",
            "examples": [
              "Collaborative tools (Google Docs, Padlet) enable peer feedback pedagogy",
              "LMS discussion forums support asynchronous Socratic dialogue",
              "Polling tools (Mentimeter, Poll Everywhere) enable real-time formative assessment",
              "Screen recording (Loom, Screencastify) supports flipped classroom model",
              "Breakout rooms (Zoom) enable small-group collaborative learning online"
            ],
            "emergentProperty": "Technology enables pedagogies impossible without it (e.g., massive peer review)",
            "constraints": "Technology also constrains - synchronous video limits spontaneous movement",
            "metaconcept": "m2:Constraint + m2:Affordance",
            "note": "TPK is pedagogy-agnostic about content (works across subjects)"
          }
        ],
        "tripleIntersection": {
          "@id": "m0:TPACK_Core",
          "name": "TPACK - Full Integration",
          "formula": "TK ∩ PK ∩ CK",
          "definition": "Synergistic knowledge enabling technology-integrated teaching of specific content with appropriate pedagogy. The 'sweet spot' where all three domains align.",
          "examples": [
            {
              "subject": "Mathematics - Calculus",
              "content": "Limit concept (CK)",
              "pedagogy": "Constructivist exploration (PK)",
              "technology": "Desmos graphing calculator (TK)",
              "tpack": "Students manipulate function parameters in Desmos to discover epsilon-delta definition of limit through guided inquiry. Technology affords dynamic visualization (TCK), inquiry pedagogy is scaffolded (TPK), and limit concept emerges through student construction (PCK). All three synergize."
            },
            {
              "subject": "History - World War I",
              "content": "Causes of WWI (CK)",
              "pedagogy": "Historical thinking - multiple perspectives (PK)",
              "technology": "Digital primary source archives + timeline tools (TK)",
              "tpack": "Students analyze digitized primary sources from multiple nations (letters, newspapers, government documents) using timeline visualization to construct causal arguments. Technology provides access to authentic sources (TCK), collaborative annotation supports argumentation pedagogy (TPK), and students develop historical empathy through multiple perspectives (PCK)."
            },
            {
              "subject": "Biology - Evolution",
              "content": "Natural selection mechanism (CK)",
              "pedagogy": "Inquiry-based modeling (PK)",
              "technology": "NetLogo agent-based modeling (TK)",
              "tpack": "Students build computational models of populations with variation, inheritance, selection. They run experiments manipulating parameters (mutation rate, selection pressure) to discover emergent evolutionary patterns. Computational modeling makes invisible process visible (TCK), inquiry pedagogy scaffolded through guided parameter exploration (TPK), deep understanding of mechanism emerges (PCK)."
            }
          ],
          "emergentProperty": "TPACK transcends component sum - creates NEW instructional possibilities",
          "metaconcept": "m2:Synergy (triple)",
          "synergyFormula": "TPACK = T ⊗ P ⊗ C where TPACK >> T + P + C"
        }
      },
      "m0:balancePrinciple": {
        "metaconcept": "m2:Balance",
        "formula": "A⊗S⊗F",
        "description": "Effective TPACK requires BALANCE between the three knowledge domains. Over-emphasis on any one dimension creates imbalance.",
        "imbalancePatterns": [
          {
            "pattern": "Technology-driven (TK >> PK, CK)",
            "symptom": "Cool tools with no pedagogical purpose or content alignment",
            "example": "Using VR headsets just because they're new, without learning objective",
            "result": "Technology as distraction, not enhancement"
          },
          {
            "pattern": "Pedagogy-driven (PK >> TK, CK)",
            "symptom": "Generic activities disconnected from content or technological affordances",
            "example": "Group work for sake of collaboration, ignoring subject matter depth",
            "result": "Engagement without learning"
          },
          {
            "pattern": "Content-driven (CK >> PK, TK)",
            "symptom": "Transmission model - technology as delivery vehicle, pedagogy ignored",
            "example": "Lecture videos with no interaction, assessment, or active learning",
            "result": "Digital lecture hall (missed opportunities)"
          }
        ],
        "balancedApproach": {
          "principle": "T, P, C must compensate and support each other",
          "example": "If content is complex (high CK demand), pedagogy must scaffold (high PK) and technology must simplify representation (appropriate TK)",
          "equilibrium": "Teacher adjusts T, P, C dynamically based on student needs, learning goals, context"
        },
