  },
  "@graph": [
    {
      "@id": "m2:Component",
      "@type": [
        "owl:NamedIndividual",
        "m2:MetaConcept"
      ],
      "rdfs:label": "Component",
      "rdfs:comment": "Identifiable elementary part of a system with specific structural role and informational signature. Components are distinguishable entities that constitute a whole through their organization and interactions.",
      "m2:hasCategory": "m2:Structural",
      "m2:hasTensorFormula": "S⊗I",
      "m2:hasDominantM3": [
        "m3:Structure",
        "m3:Information"
      ],
      "m2:hasEpistemicGap": 0.2,
      "m2:hasPolarity": "neutral",
      "m2:hasExample": [
        "Fuel in Fire Triangle (chemistry)",
        "Resistor in electrical circuit (electronics)",
        "Enzyme in metabolic pathway (biology)",
        "Player in sports team (sociology)",
        "CPU in computer system (computing)",
        "Brick in construction (architecture)",
        "Atom in molecule (chemistry)",
        "Red channel in RGB color model (optics)"
      ],
      "m2:distinctionNote": "Component differs from Module (m2:Modularity) by emphasis: Component is an elementary part with identity and function, while Module is a semi-autonomous functional unit. Component differs from Node (network topology) by being domain-general rather than network-specific.",
      "m2:useCases": [
        "System decomposition analysis",
        "Bill of materials (BOM)",
        "Compositional hierarchies",
        "Parts-whole relationships"
      ],
      "rdfs:seeAlso": [
        "m2:Modularity",
        "m2:Composition",
        "m2:Node",
        "m2:System"
      ],
      "m2:theoreticalJustification": "Component fills a critical gap in M2 ontology: the concept of identifiable parts that compose systems. While Composition (M2) describes the operation of aggregating parts, Component describes the parts themselves. This metaconcept emerged from poclet analysis (Fire Triangle: Fuel, O₂, Heat as components).",
      "m2:addedVersion": "8.1.0",
      "m2:addedDate": "2026-01-17",
      "m2:addedReason": "Gap identified during Fire Triangle poclet analysis - required to model elementary constituents of composite systems"
    }
  ]
}
